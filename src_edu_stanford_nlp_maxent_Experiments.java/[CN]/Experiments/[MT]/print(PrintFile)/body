{
  pf.println(" Experiments : ");
  for (int i=0; i < size(); i++) {
    pf.println(vArray[i][0] + " : " + vArray[i][1]);
  }
  pf.println(" p(x) ");
  for (int i=0; i < xSize; i++) {
    pf.println(i + " : " + ptildeX(i));
  }
  pf.println(" p(y) ");
  for (int i=0; i < ySize; i++) {
    pf.println(i + " : " + ptildeY(i));
  }
}
