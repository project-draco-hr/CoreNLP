{
  try {
    OutDataStreamFile rF=new OutDataStreamFile(filename);
    rF.writeInt(xSize);
    rF.writeInt(ySize);
    rF.writeInt(vArray.length);
    for (int i=0; i < xSize; i++) {
      rF.writeInt(px[i]);
    }
    for (int j=0; j < ySize; j++) {
      rF.writeInt(py[j]);
    }
    for (int i=0; i < xSize; i++) {
      for (int j=0; j < dim; j++) {
        rF.writeInt(pxy[i][j]);
      }
    }
    rF.close();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
