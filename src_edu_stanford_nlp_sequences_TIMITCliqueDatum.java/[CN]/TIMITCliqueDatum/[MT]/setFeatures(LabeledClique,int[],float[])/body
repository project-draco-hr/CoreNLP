{
  System.err.println("created");
  featureMap.put(lc,features);
  if (keySet != null) {
    keySet.add(lc);
  }
  if (this.values != null) {
    for (int i=0; i < this.values.length; i++) {
      if (this.values[i] != values[i]) {
        throw new RuntimeException("values arrays must match!");
      }
    }
  }
  this.values=values;
}
