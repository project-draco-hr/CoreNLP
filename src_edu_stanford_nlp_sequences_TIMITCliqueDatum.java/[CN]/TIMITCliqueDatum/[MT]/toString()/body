{
  StringBuilder sb=new StringBuilder();
  sb.append("TIMITCliqueDatum: ");
  sb.append("\n\tvalues: ");
  if (values == null) {
    sb.append("null");
  }
 else {
    for (int i=0; i < values.length && i < 5; i++) {
      sb.append(values[i] + " ");
    }
  }
  sb.append("\n\tfeatureMap: " + featureMap);
  return sb.toString();
}
