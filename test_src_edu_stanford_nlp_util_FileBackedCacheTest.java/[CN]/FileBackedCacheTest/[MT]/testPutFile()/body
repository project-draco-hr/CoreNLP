{
  cache.put("key","value");
  assertEquals("value",cache.put("key","valueReplaced"));
  assertEquals("valueReplaced",cache.get("key"));
  cache.put("key","value");
  cache.clear();
  assertEquals("value",cache.put("key","valueReplaced"));
  cache.clear();
  assertEquals("valueReplaced",cache.get("key"));
  assertTrue(FileBackedCache.locksHeld().isEmpty());
}
