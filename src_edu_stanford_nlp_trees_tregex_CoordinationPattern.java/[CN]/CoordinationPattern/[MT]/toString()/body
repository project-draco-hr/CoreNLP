{
  StringBuilder sb=new StringBuilder();
  if (isConj) {
    for (    TregexPattern node : children) {
      sb.append(node.toString());
    }
  }
 else {
    sb.append('[');
    for (Iterator<TregexPattern> iter=children.iterator(); iter.hasNext(); ) {
      TregexPattern node=iter.next();
      sb.append(node.toString());
      if (iter.hasNext()) {
        sb.append(" |");
      }
    }
    sb.append(']');
  }
  return sb.toString();
}
