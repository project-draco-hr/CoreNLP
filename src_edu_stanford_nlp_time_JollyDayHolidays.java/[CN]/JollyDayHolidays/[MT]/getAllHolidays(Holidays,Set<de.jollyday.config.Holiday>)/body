{
  for (  Method m : holidays.getClass().getMethods()) {
    if (isGetter(m) && m.getReturnType() == List.class) {
      try {
        List l=(List)m.invoke(holidays);
        allHolidays.addAll(l);
      }
 catch (      Exception e) {
        throw new RuntimeException("Cannot create set of holidays.",e);
      }
    }
  }
}
