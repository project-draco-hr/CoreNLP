{
  if (lexparser == null) {
synchronized (LexicalizedParserServerITest.class) {
      if (lexparser == null) {
        lexparser=LexicalizedParser.loadModel(lexmodel);
      }
      if (srparser == null) {
        srparser=ShiftReduceParser.loadModel(srmodel,"-preTag","-taggerSerializedFile",tagger);
      }
    }
  }
}
