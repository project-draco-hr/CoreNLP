{
  int[] orig=new int[args.length];
  for (int i=0; i < orig.length; i++) {
    orig[i]=Integer.parseInt(args[i]);
  }
  for (  int i : gapDecode(gapEncode(orig))) {
    System.err.print(i + " ");
  }
  System.err.println();
  for (  int i : deltaDecode(deltaEncode(orig))) {
    System.err.print(i + " ");
  }
  System.err.println();
}
