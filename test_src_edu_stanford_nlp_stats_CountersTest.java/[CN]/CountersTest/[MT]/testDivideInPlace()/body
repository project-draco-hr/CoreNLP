{
  TwoDimensionalCounter<String,String> a=new TwoDimensionalCounter<String,String>();
  a.setCount("a","b",1);
  a.setCount("a","c",1);
  a.setCount("c","a",1);
  a.setCount("c","b",1);
  Counters.divideInPlace(a,a.totalCount());
  assertEquals(1.0,a.totalCount());
  assertEquals(0.25,a.getCount("a","b"));
}
