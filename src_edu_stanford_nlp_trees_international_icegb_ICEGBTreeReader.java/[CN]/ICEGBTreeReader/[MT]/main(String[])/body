{
  try {
    Tree t;
    ICEGBTreeReader tr=new ICEGBTreeReader(new BufferedReader(new InputStreamReader(new FileInputStream(args[0]),"UTF-8")));
    t=tr.readTree();
    if (debug)     outputTree(t);
    System.err.println("===================================");
    while (t != null) {
      t=tr.readTree();
      if (debug)       outputTree(t);
      System.err.println("===================================");
    }
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
  }
}
