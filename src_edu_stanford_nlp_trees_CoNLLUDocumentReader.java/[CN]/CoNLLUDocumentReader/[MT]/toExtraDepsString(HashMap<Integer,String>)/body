{
  StringBuffer sb=new StringBuffer();
  boolean first=true;
  List<Integer> sortedKeys=new ArrayList<>(extraDeps.keySet());
  Collections.sort(sortedKeys);
  for (  Integer key : sortedKeys) {
    if (!first) {
      sb.append("|");
    }
 else {
      first=false;
    }
    sb.append(key).append(":").append(extraDeps.get(key));
  }
  if (first) {
    sb.append("_");
  }
  return sb.toString();
}
