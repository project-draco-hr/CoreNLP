{
  int numNonzeros=0;
  int len=tags.getSize();
  nonzeros=new boolean[tags.getSize()];
  for (int i=0; i < len; i++) {
    for (int j=0; j < len; j++) {
      if (cMatrix[i][j] > 20) {
        nonzeros[i]=true;
        numNonzeros++;
        break;
      }
    }
  }
  System.out.println(" Number of non zeros " + numNonzeros);
}
