{
  startTrack("Thread " + theI + " ("+ Thread.currentThread().getId()+ ")");
  for (int time=0; time < 5; time++) {
    log("tick " + time + " from "+ theI+ " ("+ Thread.currentThread().getId()+ ")");
    try {
      Thread.sleep(50);
    }
 catch (    Exception e) {
    }
  }
  endTrack("Thread " + theI + " ("+ Thread.currentThread().getId()+ ")");
  finishThread();
}
