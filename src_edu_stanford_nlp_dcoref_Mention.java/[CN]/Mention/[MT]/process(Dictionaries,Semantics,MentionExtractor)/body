{
  setHeadString();
  setType(dict);
  setNERString();
  List<String> mStr=getMentionString();
  setNumber(dict);
  setGender(dict,getGenderCount(dict,mStr));
  setAnimacy(dict);
  setPerson(dict);
  setDiscourse();
  headIndexedWord=dependency.getNodeByIndexSafe(headWord.index());
  if (semantics != null)   setSemantics(dict,semantics,mentionExtractor);
}
