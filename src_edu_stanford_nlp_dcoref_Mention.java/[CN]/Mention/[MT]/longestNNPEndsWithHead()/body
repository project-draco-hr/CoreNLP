{
  String ret="";
  for (int i=headIndex; i >= startIndex; i--) {
    String pos=sentenceWords.get(i).get(PartOfSpeechAnnotation.class);
    if (!pos.startsWith("NNP"))     break;
    if (!ret.equals(""))     ret=" " + ret;
    ret=sentenceWords.get(i).get(TextAnnotation.class) + ret;
  }
  return ret;
}
