{
  p=pattern;
  this.op=op;
  BufferedReader br=new BufferedReader(in);
  StringBuilder b=new StringBuilder();
  try {
    for (String line; (line=br.readLine()) != null; ) {
      b.append(line).append('\n');
    }
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
  m=p.matcher(b.toString());
  setNext();
}
