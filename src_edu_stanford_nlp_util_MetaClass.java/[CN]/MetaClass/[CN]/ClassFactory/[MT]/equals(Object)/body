{
  if (o instanceof ClassFactory) {
    ClassFactory other=(ClassFactory)o;
    if (!this.cl.equals(other.cl))     return false;
    for (int i=0; i < classParams.length; i++) {
      if (!this.classParams[i].equals(other.classParams[i]))       return false;
    }
    return true;
  }
 else {
    return false;
  }
}
