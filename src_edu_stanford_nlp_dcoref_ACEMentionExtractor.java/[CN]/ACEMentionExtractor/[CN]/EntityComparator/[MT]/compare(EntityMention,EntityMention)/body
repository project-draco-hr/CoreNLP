{
  if (m1.getExtentTokenStart() > m2.getExtentTokenStart())   return 1;
 else   if (m1.getExtentTokenStart() < m2.getExtentTokenStart())   return -1;
 else   if (m1.getExtentTokenEnd() > m2.getExtentTokenEnd())   return -1;
 else   if (m1.getExtentTokenEnd() < m2.getExtentTokenEnd())   return 1;
 else   return 0;
}
