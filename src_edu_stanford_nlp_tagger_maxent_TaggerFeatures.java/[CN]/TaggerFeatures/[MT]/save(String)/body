{
  try {
    OutDataStreamFile rF=new OutDataStreamFile(filename);
    rF.writeInt(xIndexed.length);
    for (    int aXIndexed : xIndexed) {
      rF.writeInt(aXIndexed);
    }
    rF.writeInt(size());
    for (int i=0; i < size(); i++) {
      get(i).save(rF);
    }
    rF.close();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
