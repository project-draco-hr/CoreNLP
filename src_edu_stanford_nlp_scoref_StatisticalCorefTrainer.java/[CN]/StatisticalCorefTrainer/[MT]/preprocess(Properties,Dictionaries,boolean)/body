{
  runDocumentProcessor(props,dictionaries,new DatasetBuilder(StatisticalCorefProperties.minClassImbalance(props),StatisticalCorefProperties.minTrainExamplesPerDocument(props)));
  runDocumentProcessor(props,dictionaries,new MetadataWriter(isTrainSet));
  runDocumentProcessor(props,dictionaries,new FeatureExtractorRunner(props,dictionaries));
}
