{
  try {
    Map<E,Set<String>> sents=new HashMap<E,Set<String>>();
    for (    E p : patterns) {
      Set<String> sentids=queryIndexGetSentences(p.getRelevantWords());
      sents.put(p,sentids);
    }
    return sents;
  }
 catch (  ParseException e) {
    throw new RuntimeException(e);
  }
catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
