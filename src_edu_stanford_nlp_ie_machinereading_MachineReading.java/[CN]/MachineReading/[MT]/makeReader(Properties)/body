{
  try {
    if (reader == null) {
      try {
        reader=datasetReaderClass.getConstructor(Properties.class).newInstance(props);
      }
 catch (      java.lang.NoSuchMethodException e) {
        reader=datasetReaderClass.getConstructor().newInstance();
      }
    }
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
  reader.setUseNewHeadFinder(useNewHeadFinder);
  return reader;
}
