{
  if (position > 0) {
    if (sequence[position - 1] == sequence[position]) {
      return entities[position - 1] == entities[position];
    }
  }
  if (position < sequence.length - 1) {
    if (sequence[position + 1] == sequence[position]) {
      return entities[position] == entities[position + 1];
    }
  }
  return false;
}
