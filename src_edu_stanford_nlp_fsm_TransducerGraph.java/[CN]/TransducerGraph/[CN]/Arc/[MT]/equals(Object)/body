{
  if (o == this) {
    return true;
  }
  if (!(o instanceof Arc)) {
    return false;
  }
  Arc a=(Arc)o;
  return ((sourceNode == null ? a.sourceNode == null : sourceNode.equals(a.sourceNode)) && (targetNode == null ? a.targetNode == null : targetNode.equals(a.targetNode)) && (input == null ? a.input == null : input.equals(a.input)));
}
