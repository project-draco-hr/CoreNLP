{
  int pathLength=(int)(r.nextGaussian() * pathLengthVariance + pathLengthMean);
  Object input;
  List path;
  for (int i=0; i < numPaths; i++) {
    path=new ArrayList();
    for (int j=0; j < pathLength; j++) {
      input=Integer.valueOf(r.nextInt(numInputs));
      path.add(input);
    }
    pathList.add(path);
  }
  return createGraphFromPaths(pathList,-1);
}
