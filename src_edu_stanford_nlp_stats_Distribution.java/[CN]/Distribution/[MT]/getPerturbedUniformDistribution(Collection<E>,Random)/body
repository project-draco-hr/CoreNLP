{
  Distribution<E> norm=new Distribution<E>();
  norm.counter=new ClassicCounter<E>();
  norm.numberOfKeys=s.size();
  norm.reservedMass=0;
  double total=s.size();
  double prob=1.0 / total;
  double stdev=prob / 1000.0;
  for (  E key : s) {
    norm.counter.setCount(key,prob + (r.nextGaussian() * stdev));
  }
  return norm;
}
