{
  double d=Math.random();
  Set<E> s=prior.keySet();
  for (  E o : s) {
    d-=probabilityOf(o);
    if (d < 0) {
      return o;
    }
  }
  System.err.println("ERROR: Distribution sums to less than 1");
  System.err.println("Sampled " + d + "      sum is "+ totalCount());
  throw new RuntimeException("");
}
