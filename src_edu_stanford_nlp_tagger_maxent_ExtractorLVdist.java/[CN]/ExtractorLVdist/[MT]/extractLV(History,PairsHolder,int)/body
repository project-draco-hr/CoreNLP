{
  int start=h.start;
  String lastverb="NA";
  int current=h.current;
  int index=current - 1;
  while ((index >= start) && (index >= current - bound)) {
    String tag=h.pairs.getTag(index);
    if (tag.startsWith("VB")) {
      lastverb=h.pairs.getWord(index) + '#' + (current - index);
      break;
    }
    if (tag.startsWith(",")) {
      break;
    }
    index--;
  }
  return lastverb;
}
