{
  if (depth == ambiguities.size()) {
    path.terminate();
    return;
  }
  GeneIDPair curGene=ambiguities.get(depth);
  for (Iterator<String> iter=curGene.ids.iterator(); iter.hasNext(); ) {
    String id=iter.next();
    path.push(id);
    disambiguate(ambiguities,path,depth + 1);
    path.pop();
  }
}
