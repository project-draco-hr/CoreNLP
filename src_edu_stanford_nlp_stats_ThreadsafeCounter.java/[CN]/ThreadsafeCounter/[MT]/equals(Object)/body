{
  if (this == o) {
    return true;
  }
 else   if (!(o instanceof ThreadsafeCounter)) {
    return false;
  }
 else {
    final ThreadsafeCounter<E> other=(ThreadsafeCounter<E>)o;
synchronized (map) {
synchronized (other.map) {
        return totalCount == other.totalCount && map.equals(other.map);
      }
    }
  }
}
