{
  if (mappingFiles.contains(";") && mappingFiles.contains(",")) {
    return SEMICOLON_DELIMITERS_PATTERN.split(mappingFiles);
  }
 else   if (mappingFiles.contains(",")) {
    return COMMA_DELIMITERS_PATTERN.split(mappingFiles);
  }
 else {
    return SEMICOLON_DELIMITERS_PATTERN.split(mappingFiles);
  }
}
