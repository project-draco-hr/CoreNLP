{
  Map<String,Integer> map=new HashMap<String,Integer>();
  for (int i=0; i < headerFields.length; i++) {
    String field=headerFields[i];
    if (map.containsKey(field)) {
      throw new IllegalArgumentException("Duplicate header field: " + field);
    }
    map.put(field,i);
  }
  return map;
}
