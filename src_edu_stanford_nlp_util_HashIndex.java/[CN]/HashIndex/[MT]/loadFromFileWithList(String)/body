{
  Index<String> index=new HashIndex<String>();
  BufferedReader br=null;
  try {
    br=new BufferedReader(new FileReader(file));
    for (String line; (line=br.readLine()) != null; ) {
      index.add(line.trim());
    }
    br.close();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
 finally {
    if (br != null) {
      try {
        br.close();
      }
 catch (      IOException ioe) {
      }
    }
  }
  return index;
}
