{
  if (lastValues != null && lastValues.length == values.length) {
    boolean match=true;
    for (int i=0; i < values.length; i++) {
      if (values[i] != lastValues[i]) {
        match=false;
        break;
      }
    }
    if (match) {
      values=lastValues;
    }
  }
  Features f=Features.valueOf(features,values);
  featureMap.put(lc,f);
  lastValues=values;
}
