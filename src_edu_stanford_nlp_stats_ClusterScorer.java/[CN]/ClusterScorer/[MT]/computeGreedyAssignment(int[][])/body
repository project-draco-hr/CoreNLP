{
  int[] assignment=new int[confusionMatrix[0].length];
  for (int j=0; j < confusionMatrix[0].length; j++) {
    int maxRow=-1;
    int maxNum=0;
    for (int i=0; i < confusionMatrix.length; i++) {
      if (confusionMatrix[i][j] >= maxNum) {
        maxNum=confusionMatrix[i][j];
        maxRow=i;
      }
    }
    assignment[j]=maxRow;
  }
  return assignment;
}
