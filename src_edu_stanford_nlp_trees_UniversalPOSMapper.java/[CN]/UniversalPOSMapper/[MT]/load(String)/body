{
  loaded=true;
  try {
    BufferedReader reader=IOUtils.getBufferedReaderFromClasspathOrFileSystem(filename);
    operations=Tsurgeon.getOperationsFromReader(reader,new TregexPatternCompiler());
    reader.close();
  }
 catch (  IOException e) {
    System.err.printf("%s: Warning - could not load Tsurgeon file from %s.%n",UniversalPOSMapper.class.getSimpleName(),filename);
  }
}
