{
  loaded=true;
  try {
    URL url=IOUtils.class.getClassLoader().getResource(filename);
    if (url == null) {
      System.err.printf("%s: Warning - could not load Tsurgeon file from %s.%n",UniversalPOSMapper.class.getSimpleName(),filename);
      return;
    }
    String path=url.getPath();
    operations=Tsurgeon.getOperationsFromFile(path,"UTF-8",new TregexPatternCompiler());
  }
 catch (  IOException e) {
    System.err.printf("%s: Warning - could not load Tsurgeon file from %s.%n",UniversalPOSMapper.class.getSimpleName(),filename);
  }
}
