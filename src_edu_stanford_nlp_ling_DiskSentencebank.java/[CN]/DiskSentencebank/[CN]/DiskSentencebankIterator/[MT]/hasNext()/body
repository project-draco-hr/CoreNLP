{
  while (fileUpto < files.size()) {
    if (sentenceUpto < currentFileSentences.size()) {
      return true;
    }
 else {
      currentFileSentences.clear();
      fileUpto++;
      sentenceUpto=0;
      if (fileUpto < files.size()) {
        currentFileSentences.loadPath(files.get(fileUpto));
      }
    }
  }
  return false;
}
