{
  int numColors=tags.size() - 1;
  Color[] colors=getNColors(numColors);
  Map<String,Color> result=new HashMap<String,Color>();
  int i=0;
  for (  String tag : tags) {
    if (backgroundSymbol.equals(tag)) {
      continue;
    }
    if (result.get(tag) != null) {
      continue;
    }
    result.put(tag,colors[i++]);
  }
  return result;
}
