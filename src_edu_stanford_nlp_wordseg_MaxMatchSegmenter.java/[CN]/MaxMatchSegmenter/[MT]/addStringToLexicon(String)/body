{
  if (str.equals("")) {
    System.err.println("WARNING: blank line in lexicon");
  }
 else   if (str.contains(" ")) {
    System.err.println("WARNING: word with space in lexicon");
  }
 else {
    if (excludeChar(str)) {
      printlnErr("skipping word: " + str);
      return;
    }
    words.add(str);
  }
}
