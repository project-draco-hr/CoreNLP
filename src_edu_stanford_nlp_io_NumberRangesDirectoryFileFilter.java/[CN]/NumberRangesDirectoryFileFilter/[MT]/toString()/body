{
  StringBuilder sb;
  if (recursively) {
    sb=new StringBuilder("recursively ");
  }
 else {
    sb=new StringBuilder();
  }
  for (Iterator<Pair<Integer,Integer>> it=ranges.iterator(); it.hasNext(); ) {
    Pair<Integer,Integer> p=it.next();
    int low=p.first().intValue();
    int high=p.second().intValue();
    if (low == high) {
      sb.append(low);
    }
 else {
      sb.append(low);
      sb.append("-");
      sb.append(high);
    }
    if (it.hasNext()) {
      sb.append(",");
    }
  }
  return sb.toString();
}
