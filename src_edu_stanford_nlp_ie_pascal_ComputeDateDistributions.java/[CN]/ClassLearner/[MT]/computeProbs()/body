{
  boolean hasZeroEntry=false;
  for (int i=0; i < counts.length; i++) {
    if (counts[i] == 0) {
      hasZeroEntry=true;
    }
  }
  if (hasZeroEntry) {
    doAddOneSmoothing();
  }
  probs=new double[counts.length];
  for (int i=0; i < counts.length; i++) {
    probs[i]=((double)counts[i]) / ((double)total);
  }
}
