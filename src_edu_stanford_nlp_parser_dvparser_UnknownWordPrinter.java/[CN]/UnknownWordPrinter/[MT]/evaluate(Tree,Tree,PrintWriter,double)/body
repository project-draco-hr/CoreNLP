{
  List<Label> words=guess.yield();
  int pos=0;
  for (  Label word : words) {
    ++pos;
    SimpleMatrix wv=model.getWordVector(word.value());
    if (wv == unk || wv.equals(unk)) {
      pw.printf("  Unknown word in position %d: %s\n",pos,word.value());
      unkWords.add(word.value());
    }
  }
}
