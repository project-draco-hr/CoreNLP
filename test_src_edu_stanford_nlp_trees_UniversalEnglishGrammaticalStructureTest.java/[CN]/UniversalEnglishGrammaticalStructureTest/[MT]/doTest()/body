{
switch (this.type) {
case BASIC:
    testBasicRelation();
  break;
case COPULA_HEAD:
testBasicRelationWithCopulaAsHead();
break;
case NON_COLLAPSED:
testNonCollapsedRelation();
break;
case NON_COLLAPSED_SEPARATOR:
break;
case COLLAPSED:
testCollapsedRelation();
break;
case CC_PROCESSED:
testCCProcessedRelation();
break;
case COPY_NODES:
testCollapsedRelation();
break;
}
}
