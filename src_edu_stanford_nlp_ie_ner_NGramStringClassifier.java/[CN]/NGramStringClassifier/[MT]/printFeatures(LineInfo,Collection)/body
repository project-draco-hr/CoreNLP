{
  if (cliqueWriter != null) {
    cliqueWriter.print(wi.word + " " + wi.tag+ " "+ wi.answer);
    cliqueWriter.print("\t");
    boolean first=true;
    for (Iterator it=features.iterator(); it.hasNext(); ) {
      String feat=(String)it.next();
      if (first) {
        first=false;
      }
 else {
        cliqueWriter.print(" ");
      }
      cliqueWriter.print(feat);
    }
    cliqueWriter.println();
  }
}
