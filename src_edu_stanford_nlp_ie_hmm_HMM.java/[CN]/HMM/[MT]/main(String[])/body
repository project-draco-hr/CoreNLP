{
  String filename="/u/nlp/data/iedata/hmmtest";
  String field="location";
  if (args.length > 0) {
    filename=args[0];
    if (args.length > 1) {
      field=args[1];
    }
  }
  new HMM(REGULAR_HMM).unitTestTraining1(new Corpus(filename,field));
  new HMM(Structure.fsnlpHMM(),REGULAR_HMM).unitTestTraining2();
}
