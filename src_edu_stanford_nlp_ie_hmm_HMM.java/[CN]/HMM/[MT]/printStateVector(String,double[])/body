{
  NumberFormat nf=NumberFormat.getNumberInstance();
  nf.setMaximumFractionDigits(3);
  System.err.print(name + " = {");
  for (int i=0; i < states.length; i++) {
    System.err.print(i + "=" + nf.format(states[i]));
    if (i != states.length - 1) {
      System.err.print(",");
    }
  }
  System.err.println("}");
}
