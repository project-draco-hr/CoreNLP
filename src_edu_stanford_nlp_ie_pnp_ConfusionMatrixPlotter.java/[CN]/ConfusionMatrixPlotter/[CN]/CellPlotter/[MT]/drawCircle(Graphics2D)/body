{
  double r=getScaledRadius();
  int s=getCellSize();
  double cx=s / 2.0f;
  double cy=s / 2.0f;
  Ellipse2D.Double circle=new Ellipse2D.Double(cx - r,cy - r,2 * r,2 * r);
  g2.setPaint(fillColor);
  g2.setStroke(new BasicStroke(s * 0.02f));
  g2.fill(circle);
  g2.setColor(strokeColor);
  g2.draw(circle);
}
