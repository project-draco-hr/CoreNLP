{
  for (int i=0; i < PascalTemplate.fields.length; i++) {
    HashSet hm=new HashSet();
    for (int j=0; j < numValues[i]; j++) {
      if (hm.contains(values[i][j].trim())) {
        for (int k=j; k < numValues[i] - 1; k++) {
          values[i][k]=values[i][k + 1];
        }
        numValues[i]--;
        j--;
      }
 else {
        hm.add(values[i][j].trim());
      }
    }
  }
}
