{
  int it=Integer.parseInt(args[0]);
  int sz=Integer.parseInt(args[1]);
  boolean cached=Boolean.parseBoolean(args[2]);
  String[] tagsets=new String[]{"CC","CD","JJ","LB","NN","VBD","VBZ","DT"};
  String[] conj=new String[sz];
  Random r=new Random();
  for (int i=0; i < it; ++i) {
    for (int j=0; j < conj.length; ++j)     conj[j]=tagsets[r.nextInt(tagsets.length)];
    if (cached)     StringBuildMemoizer.toString(conj);
 else     StringBuildMemoizer.uncachedToString(conj);
  }
}
