{
  try {
    corefProperties=props;
    dictionaries=new Dictionaries(props);
    headFinder=getHeadFinder(props);
    md=getMentionFinder(props,headFinder);
    log.info("Using mention detector type: " + mdName);
    mentionAnnotatorRequirements.addAll(Arrays.asList(CoreAnnotations.TokensAnnotation.class,CoreAnnotations.SentencesAnnotation.class,CoreAnnotations.PartOfSpeechAnnotation.class,CoreAnnotations.NamedEntityTagAnnotation.class,CoreAnnotations.IndexAnnotation.class,CoreAnnotations.TextAnnotation.class,CoreAnnotations.ValueAnnotation.class,SemanticGraphCoreAnnotations.BasicDependenciesAnnotation.class,SemanticGraphCoreAnnotations.EnhancedDependenciesAnnotation.class));
  }
 catch (  Exception e) {
    log.info("Error with building coref mention annotator!");
  }
}
