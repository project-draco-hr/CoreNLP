{
  try {
    corefProperties=props;
    dictionaries=new Dictionaries(props);
    headFinder=getHeadFinder(props);
    md=getMentionFinder(props,headFinder);
    System.err.println("Using mention detector: " + mdName + " which requires these parses: "+ mentionAnnotatorRequirements);
    mentionAnnotatorRequirements.addAll(Arrays.asList(CoreAnnotations.TokensAnnotation.class,CoreAnnotations.SentencesAnnotation.class,CoreAnnotations.PartOfSpeechAnnotation.class,CoreAnnotations.NamedEntityTagAnnotation.class,SemanticGraphCoreAnnotations.BasicDependenciesAnnotation.class,SemanticGraphCoreAnnotations.CollapsedDependenciesAnnotation.class,SemanticGraphCoreAnnotations.CollapsedCCProcessedDependenciesAnnotation.class));
  }
 catch (  Exception e) {
    System.err.println("Error with building coref mention annotator!");
  }
}
