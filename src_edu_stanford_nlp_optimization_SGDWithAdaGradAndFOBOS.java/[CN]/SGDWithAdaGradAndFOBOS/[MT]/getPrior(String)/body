{
  if (priorType.equals("none"))   return Prior.NONE;
 else   if (priorType.equals("lasso"))   return Prior.LASSO;
 else   if (priorType.equals("ridge"))   return Prior.RIDGE;
 else   if (priorType.equals("gaussian"))   return Prior.GAUSSIAN;
 else   if (priorType.equals("ae-lasso"))   return Prior.aeLASSO;
 else   if (priorType.equals("g-lasso"))   return Prior.gLASSO;
 else   if (priorType.equals("sg-lasso"))   return Prior.sgLASSO;
 else   throw new IllegalArgumentException("prior type " + priorType + " not recognized; supported priors "+ "are: lasso, ridge, gaussian, ae-lasso, g-lasso, and sg-lasso");
}
