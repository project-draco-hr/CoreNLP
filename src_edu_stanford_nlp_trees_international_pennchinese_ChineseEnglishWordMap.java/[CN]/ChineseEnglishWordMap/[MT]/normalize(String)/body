{
  String origT;
  if (DEBUG) {
    origT=t;
  }
  if (!this.normalized) {
    return t;
  }
  for (  String punc : punctuations) {
    t=t.replaceAll(punc,"");
  }
  t=t.trim();
  if (DEBUG && !origT.equals(t)) {
    System.err.println("orig=" + origT);
    System.err.println("norm=" + t);
  }
  return t;
}
