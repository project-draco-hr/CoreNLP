{
  int i=0, j=0;
  while (true) {
    int ri_i=clique.relativeIndex(i);
    int ri_j=otherClique.clique.relativeIndex(j);
    if (ri_i == ri_j + offset) {
      if (labels[i] != otherClique.labels[j]) {
        return false;
      }
      i++;
      j++;
    }
 else     if (ri_i > ri_j + offset) {
      j++;
    }
 else {
      i++;
    }
    if (i == size() || j == otherClique.size()) {
      break;
    }
  }
  return true;
}
