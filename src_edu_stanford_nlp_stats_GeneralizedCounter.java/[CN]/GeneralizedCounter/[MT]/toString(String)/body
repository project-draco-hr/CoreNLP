{
  if (param.equals("contingency")) {
    StringBuilder sb=new StringBuilder();
    for (    K obj : ErasureUtils.sortedIfPossible(topLevelKeySet())) {
      sb.append(obj);
      sb.append(" = ");
      GeneralizedCounter<K> gc=conditionalizeOnce(obj);
      sb.append(gc);
      sb.append("\n");
    }
    return sb.toString();
  }
 else   if (param.equals("sorted")) {
    StringBuilder sb=new StringBuilder();
    sb.append("{\n");
    for (    K obj : ErasureUtils.sortedIfPossible(topLevelKeySet())) {
      sb.append(obj);
      sb.append(" = ");
      GeneralizedCounter<K> gc=conditionalizeOnce(obj);
      sb.append(gc);
      sb.append("\n");
    }
    sb.append("}\n");
    return sb.toString();
  }
 else {
    return toString();
  }
}
