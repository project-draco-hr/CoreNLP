{
  try {
    String outfile=outfilePrefix;
    if (fold > 0) {
      outfile+="." + fold;
    }
    outfile+=".hmm";
    System.err.println("Serializing HMM to " + outfile + "...");
    ObjectOutputStream oos=new ObjectOutputStream(new FileOutputStream(outfile));
    oos.writeObject(hmm);
    oos.close();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
