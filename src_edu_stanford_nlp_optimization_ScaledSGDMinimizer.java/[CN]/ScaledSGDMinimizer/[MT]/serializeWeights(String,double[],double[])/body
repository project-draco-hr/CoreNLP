{
  System.err.println("Serializing weights to " + serializePath + "...");
  try {
    Weights out=new Weights(weights,diag);
    IOUtils.writeObjectToFile(out,serializePath);
  }
 catch (  Exception e) {
    System.err.println("Error serializing to " + serializePath);
    e.printStackTrace();
  }
}
