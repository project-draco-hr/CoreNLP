{
  String id=resultsTableModel.getResult(resultsTable.getSelectedRow()).getID();
  resultsTableModel.setFilter(filter);
  setStatus(resultsTableModel.getNumDocuments() + " documents.");
  int index=findDocument(id);
  if (index != -1) {
    gotoDocument(index);
  }
 else {
    gotoDocument(0);
  }
}
