{
  if (patternClass.equals(PatternFactory.PatternType.SURFACE))   return SurfacePattern.sameGenre((SurfacePattern)p1,(SurfacePattern)p2);
 else   if (patternClass.equals(PatternFactory.PatternType.DEP))   return DepPattern.sameGenre((DepPattern)p1,(DepPattern)p2);
 else   throw new UnsupportedOperationException();
}
