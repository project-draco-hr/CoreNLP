{
  if (r != null && w != null) {
    try {
      char[] buffer=new char[4096];
      int c;
      while ((c=r.read(buffer)) != -1) {
        w.write(buffer,0,c);
      }
      r.close();
      w.flush();
      w.close();
    }
 catch (    IOException e) {
      System.err.println("ConvertEncodingThread run: " + e);
    }
  }
}
