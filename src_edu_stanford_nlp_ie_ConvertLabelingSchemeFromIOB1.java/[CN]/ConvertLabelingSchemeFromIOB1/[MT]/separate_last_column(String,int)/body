{
  s=s.trim();
  String retVal[]=new String[2];
  retVal[0]="";
  retVal[1]="";
  StringTokenizer st=new StringTokenizer(s);
  if (st.countTokens() < columns) {
    retVal[0]=s;
    return retVal;
  }
  for (int i=0; i < columns - 1; i++)   retVal[0]+=st.nextToken() + " ";
  retVal[1]=st.nextToken();
  return retVal;
}
