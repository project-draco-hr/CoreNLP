{
  String retVal[]=new String[2];
  retVal[0]="";
  retVal[1]="";
  try {
    if (s.length() > 1)     retVal[0]=s.substring(0,1);
 else     retVal[0]="";
  }
 catch (  Exception e) {
    e.printStackTrace();
    retVal[0]="";
  }
  try {
    if (s.length() > 2)     retVal[1]=s.substring(1);
 else     retVal[1]="";
  }
 catch (  Exception e) {
    e.printStackTrace();
    retVal[1]="";
  }
  return retVal;
}
