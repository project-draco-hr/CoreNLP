{
  if (in == null)   return null;
  IntCounter<Object> counter=new IntCounter<Object>();
  for (  CoreMap cm : in) {
    Object obj=cm.get(key);
    if (obj != null && (ignoreSet == null || !ignoreSet.contains(obj))) {
      counter.incrementCount(obj);
    }
  }
  if (counter.size() > 0) {
    return counter.argmax();
  }
 else {
    return null;
  }
}
