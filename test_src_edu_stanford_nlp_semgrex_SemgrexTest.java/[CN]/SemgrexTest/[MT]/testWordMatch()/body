{
  runTest("{word:Bill}","[ate subj:Bill dobj:[muffins nn:blueberry]]","Bill");
  runTest("!{word:Bill}","[ate subj:Bill dobj:[muffins nn:blueberry]]","ate","muffins","blueberry");
  runTest("!{word:Fred}","[ate subj:Bill dobj:[muffins nn:blueberry]]","ate","Bill","muffins","blueberry");
  runTest("!{word:ate}","[ate subj:Bill dobj:[muffins nn:blueberry]]","Bill","muffins","blueberry");
  runTest("{word:/^(?!Bill).*$/}","[ate subj:Bill dobj:[muffins nn:blueberry]]","ate","muffins","blueberry");
  runTest("{word:/^(?!Fred).*$/}","[ate subj:Bill dobj:[muffins nn:blueberry]]","ate","Bill","muffins","blueberry");
  runTest("{word:/^(?!ate).*$/}","[ate subj:Bill dobj:[muffins nn:blueberry]]","Bill","muffins","blueberry");
  runTest("{word:muffins} >nn {word:blueberry}","[ate subj:Bill dobj:[muffins nn:blueberry]]","muffins");
  runTest("{} << {word:ate}=a","[ate subj:Bill dobj:[muffins nn:blueberry]]","Bill","muffins","blueberry");
  runTest("{} << !{word:ate}=a","[ate subj:Bill dobj:[muffins nn:blueberry]]","blueberry");
  runTest("{} << {}=a","[ate subj:Bill dobj:[muffins nn:blueberry]]","Bill","muffins","blueberry","blueberry");
}
