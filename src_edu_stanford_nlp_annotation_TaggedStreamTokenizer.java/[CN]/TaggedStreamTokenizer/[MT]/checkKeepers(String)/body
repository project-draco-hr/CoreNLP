{
  if (tag.equals("<!--") || tag.equals("-->")) {
    return !discardCommentTags;
  }
  StringTokenizer st=new StringTokenizer(tag," \t\r\n\f</>");
  if (st.hasMoreTokens()) {
    String name=st.nextToken();
    if (keeperTags.get(name.toLowerCase()) != null) {
      return true;
    }
  }
  return false;
}
