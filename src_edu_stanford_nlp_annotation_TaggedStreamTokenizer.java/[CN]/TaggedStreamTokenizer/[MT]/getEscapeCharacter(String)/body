{
  if (discardHtml) {
    return generateToken();
  }
 else {
    if (tagInScope) {
      attr=currentTag;
      sval=s;
      ttype=TT_TARGET_HTML;
      return TT_TARGET_HTML;
    }
 else {
      attr=null;
      sval=s;
      ttype=TT_BACKGROUND_HTML;
      return TT_BACKGROUND_HTML;
    }
  }
}
