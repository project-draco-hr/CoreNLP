{
  if (keeperList == null || keeperList.length() == 0) {
    return generateToken();
  }
 else   if (s.length() > 1) {
    if (tokenCode != HtmlLexerConstants.DOTPLUS) {
      return generateToken();
    }
 else     if (keeperList.indexOf('.') == -1) {
      return generateToken();
    }
  }
 else {
    if (keeperList.indexOf(s) == -1) {
      return generateToken();
    }
  }
  return getWord(s);
}
