{
  assertEquals("tokenize,ssplit,parse",StanfordCoreNLP.ensurePrerequisiteAnnotators(new String[]{"parse"}));
  assertEquals("tokenize,ssplit,pos,depparse",StanfordCoreNLP.ensurePrerequisiteAnnotators(new String[]{"depparse"}));
  assertEquals("tokenize,ssplit,pos,depparse",StanfordCoreNLP.ensurePrerequisiteAnnotators(new String[]{"depparse","tokenize"}));
  assertEquals("tokenize,ssplit,pos,lemma,depparse,natlog",StanfordCoreNLP.ensurePrerequisiteAnnotators(new String[]{"natlog","tokenize"}));
}
