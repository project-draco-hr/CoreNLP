{
  assertEquals("tokenize,ssplit,pos,lemma,depparse,natlog",StanfordCoreNLP.ensurePrerequisiteAnnotators(new String[]{"lemma","depparse","natlog"}));
  assertEquals("tokenize,ssplit,pos,depparse,lemma,natlog",StanfordCoreNLP.ensurePrerequisiteAnnotators(new String[]{"depparse","lemma","natlog"}));
  assertEquals("tokenize,ssplit,pos,lemma,ner,regexner",StanfordCoreNLP.ensurePrerequisiteAnnotators(new String[]{"ner","regexner"}));
  assertEquals("tokenize,ssplit,pos,lemma,regexner,ner",StanfordCoreNLP.ensurePrerequisiteAnnotators(new String[]{"regexner","ner"}));
}
