{
  String gnew=g;
  if (!Data.domainNGramRawFreq.containsKey(gnew)) {
    gnew=g.replaceAll(" ","");
  }
  if (!Data.domainNGramRawFreq.containsKey(gnew)) {
    gnew=g.replaceAll("-","");
  }
 else   g=gnew;
  if (!Data.domainNGramRawFreq.containsKey(gnew)) {
    System.err.println("domain count 0 for " + g);
    return 0;
  }
 else   g=gnew;
  return ((1 + Data.rawFreq.getCount(g) * Math.sqrt(Data.ratioDomainNgramFreqWithDataFreq)) / Data.domainNGramRawFreq.getCount(g));
}
