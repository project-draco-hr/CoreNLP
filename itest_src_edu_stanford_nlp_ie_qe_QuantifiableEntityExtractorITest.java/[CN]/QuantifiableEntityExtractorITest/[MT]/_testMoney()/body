{
  String[] sentences={"I have 1 dollar and 2 cents.","It cost 10 thousand million dollars."};
  ExpectedQuantity[][] expected={{new ExpectedQuantity("1 dollar","$1.00","MONEY"),new ExpectedQuantity("2 cents","$0.02","MONEY")},{new ExpectedQuantity("10 thousand million dollars","$10000000000.00","MONEY")}};
  runAndCheck("testMoney",sentences,expected);
}
