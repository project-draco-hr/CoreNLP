{
  String cword=pH.getWord(h,0);
  if (!cword.equals(thatWord)) {
    return "0";
  }
  int start=h.start;
  String lastverb="NA";
  int current=h.current;
  int index=current - 1;
  String tag;
  while (index >= start) {
    tag=pH.getTag(index);
    if (tag.startsWith("VB")) {
      lastverb=pH.getWord(index);
      break;
    }
    index--;
  }
  if (lastverb.startsWith("NA")) {
    return "3";
  }
  if (isThatComplTakingVerb(lastverb)) {
    return ("1");
  }
  return ("2");
}
