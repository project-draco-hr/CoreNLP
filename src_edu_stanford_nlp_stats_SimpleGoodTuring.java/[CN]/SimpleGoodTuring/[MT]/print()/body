{
  int i;
  System.out.printf("%6s %6s %8s %8s%n","r","n","p","p*");
  System.out.printf("%6s %6s %8s %8s%n","----","----","----","----");
  System.out.printf("%6d %6d %8.4g %8.4g%n",0,0,0.0,pZero);
  for (i=0; i < rows; ++i)   System.out.printf("%6d %6d %8.4g %8.4g%n",r[i],n[i],1.0 * r[i] / bigN,p[i]);
}
