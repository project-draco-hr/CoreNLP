{
  if (digit.matcher(word).find())   return "z0";
 else   if (nouns.contains(word))   return "ncms000";
 else   if (adjectives.contains(word))   return "aq0000";
 else   if (preps.contains(word))   return "sp000";
  System.err.println("No POS tag for " + word);
  return Character.isUpperCase(word.codePointAt(0)) ? "np00000" : "ncms000";
}
