{
  int res=comparator.compare(o1,o2);
switch (compType) {
case GT:
    return res > 0;
case LT:
  return res < 0;
case GE:
return res >= 0;
case LE:
return res <= 0;
case EQ:
return res == 0;
case NE:
return res != 0;
default :
throw new UnsupportedOperationException("Unknown compType: " + compType);
}
}
