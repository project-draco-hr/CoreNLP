{
  if (in.size() != 2) {
    return false;
  }
  if (clazz != null) {
    if (in.get(0) == null || !(clazz.isAssignableFrom(in.get(0).get().getClass()))) {
      return false;
    }
    if (in.get(1) == null || !(clazz.isAssignableFrom(in.get(1).get().getClass()))) {
      return false;
    }
  }
  return true;
}
