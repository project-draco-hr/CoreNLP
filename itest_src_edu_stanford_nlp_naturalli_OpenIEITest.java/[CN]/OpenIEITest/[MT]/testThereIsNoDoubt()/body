{
  assertExtracted(new HashSet<String>(){
{
      add("I\tam\tgreat");
    }
  }
,"There is no doubt that I am great.");
  assertExtracted(new HashSet<String>(){
{
      add("cats\thave\ttails");
    }
  }
,"There is no doubt that cats have tails.");
  assertExtracted(new HashSet<String>(){
{
    }
  }
,"There is doubt that cats have tails.");
}
