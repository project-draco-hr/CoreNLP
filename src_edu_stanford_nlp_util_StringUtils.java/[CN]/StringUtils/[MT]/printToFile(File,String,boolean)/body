{
  PrintWriter pw=null;
  try {
    Writer fw=new FileWriter(file,append);
    pw=new PrintWriter(fw);
    pw.print(message);
  }
 catch (  Exception e) {
    log.info("Exception: in printToFile " + file.getAbsolutePath());
    e.printStackTrace();
  }
 finally {
    if (pw != null) {
      pw.flush();
      pw.close();
    }
  }
}
