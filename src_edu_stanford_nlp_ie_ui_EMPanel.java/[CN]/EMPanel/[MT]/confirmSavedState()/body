{
  if (changed) {
    int response=JOptionPane.showConfirmDialog(this,"Save changes to current mediator before closing?");
    if (response == JOptionPane.YES_OPTION) {
      return (saveMediator());
    }
    return (response == JOptionPane.YES_OPTION || response == JOptionPane.NO_OPTION);
  }
  return (true);
}
