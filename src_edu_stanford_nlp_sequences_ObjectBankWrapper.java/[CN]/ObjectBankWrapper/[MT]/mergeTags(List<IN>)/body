{
  for (  IN wi : doc) {
    String answer=wi.get(AnswerAnnotation.class);
    if (answer == null) {
      continue;
    }
    if (!answer.equals(flags.backgroundSymbol)) {
      int index=answer.indexOf('-');
      if (index >= 0) {
        answer=answer.substring(index + 1);
      }
    }
    wi.set(AnswerAnnotation.class,answer);
  }
}
