{
  int n=0;
  Tree[] kids=t.children();
  for (int i=0, len=kids.length; i < len; i++) {
    String childStr=kids[i].label().value();
    if (childStr.startsWith("NP") && !childStr.contains("-TMP")) {
      n++;
    }
 else     if (englishTrain.markDitransV == 1 && childStr.startsWith("S")) {
      n++;
    }
  }
  if (n >= 2) {
    return "^2Arg";
  }
 else {
    return "";
  }
}
