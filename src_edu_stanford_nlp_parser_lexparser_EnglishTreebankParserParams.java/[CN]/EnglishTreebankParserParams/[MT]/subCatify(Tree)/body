{
  StringBuilder sb=new StringBuilder("^a");
  boolean n=false;
  boolean s=false;
  boolean p=false;
  for (int i=0; i < t.children().length; i++) {
    String childStr=t.children()[i].label().value();
    n=(n || childStr.startsWith("NP"));
    s=(s || childStr.startsWith("S"));
    p=(p || childStr.startsWith("PP"));
  }
  n=false;
  if (n) {
    sb.append("N");
  }
  if (p) {
    sb.append("P");
  }
  if (s) {
    sb.append("S");
  }
  return sb.toString();
}
