{
  boolean firstIsT=false;
  int numNNP=0;
  Tree[] kids=parent.children();
  for (int i=0; i < kids.length; i++) {
    if (kids[i].value().startsWith("NNP")) {
      if (t.equals(kids[i]) && numNNP == 0) {
        firstIsT=true;
      }
      numNNP++;
    }
  }
  return numNNP > 1 && firstIsT;
}
