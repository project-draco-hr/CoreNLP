{
  String word=w.toLowerCase();
  if (word.endsWith("ing")) {
    return "VBG";
  }
 else   if (word.endsWith("d") || word.endsWith("t")) {
    return "VBN";
  }
 else   if (word.endsWith("s")) {
    return "VBZ";
  }
 else   if (word.equals("to")) {
    return "TO";
  }
 else {
    return "VB";
  }
}
