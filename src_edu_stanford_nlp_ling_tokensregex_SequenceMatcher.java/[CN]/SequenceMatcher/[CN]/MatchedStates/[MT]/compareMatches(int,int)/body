{
  if (bid1 == bid2)   return 0;
  List<Integer> p1=branchStates.getParents(bid1);
  p1.add(bid1);
  List<Integer> p2=branchStates.getParents(bid2);
  p2.add(bid2);
  int n=Math.min(p1.size(),p2.size());
  for (int i=0; i < n; i++) {
    if (p1.get(i) < p2.get(i))     return -1;
    if (p1.get(i) > p2.get(i))     return 1;
  }
  if (p1.size() < p2.size())   return -1;
  if (p1.size() > p2.size())   return 1;
  return 0;
}
