{
  Filters.DisjFilter<T> r;
  if (filterRule instanceof Filters.DisjFilter) {
    r=(Filters.DisjFilter<T>)filterRule;
    r.addFilter(rule);
  }
 else {
    if (filterRule == null) {
      r=new Filters.DisjFilter<T>(rule);
    }
 else {
      r=new Filters.DisjFilter<T>(filterRule,rule);
    }
    filterRule=r;
  }
}
