{
  if (filterRule == null)   return expressions;
  if (expressions.size() == 0)   return expressions;
  int nfiltered=0;
  List<T> kept=new ArrayList<>(expressions.size());
  for (  T expr : expressions) {
    if (!filterRule.test(expr)) {
      kept.add(expr);
    }
 else {
      nfiltered++;
    }
  }
  if (nfiltered > 0) {
    log.debug("Filtered " + nfiltered);
  }
  return kept;
}
