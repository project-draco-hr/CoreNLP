{
  NamedEntityConfusionMatrices n=new NamedEntityConfusionMatrices();
  String filename=args[0];
  int position=0;
  int majorityOption=Integer.parseInt(args[1]);
switch (majorityOption) {
case 1:
    n.corpusLevelConfusionMatrix(filename,true,true);
  break;
case 2:
n.corpusLevelSubstringConfusionMatrix(filename,true);
break;
}
}
