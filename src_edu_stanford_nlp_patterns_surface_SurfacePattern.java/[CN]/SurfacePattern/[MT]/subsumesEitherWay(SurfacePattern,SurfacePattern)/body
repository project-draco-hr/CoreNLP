{
  boolean subsume=false;
  if (notSameGenre(p1,p2)) {
    return false;
  }
  if (p1.getOriginalNextStr().contains(p2.getOriginalNextStr()) && p1.getOriginalPrevStr().contains(p2.getOriginalPrevStr())) {
    return true;
  }
  if (p2.getOriginalNextStr().contains(p1.getOriginalNextStr()) && p2.getOriginalPrevStr().contains(p1.getOriginalPrevStr())) {
    return true;
  }
  return subsume;
}
