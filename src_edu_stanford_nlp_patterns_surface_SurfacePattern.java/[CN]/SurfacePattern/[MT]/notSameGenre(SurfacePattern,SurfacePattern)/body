{
  boolean diff=false;
  if ((p1.getNextContext() != null && p2.getNextContext() == null) || (p2.getNextContext() != null && p1.getNextContext() == null)) {
    diff=true;
    return diff;
  }
  if ((p1.getPrevContext() != null && p2.getPrevContext() == null) || (p2.getPrevContext() != null && p1.getPrevContext() == null)) {
    diff=true;
    return diff;
  }
  return diff;
}
