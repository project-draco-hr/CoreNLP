{
  if (type < -2 || type > 1000) {
    System.err.println("State: warning -- suspicious type.");
  }
  this.type=type;
  if (type < 0 && emit != null) {
    System.err.println("State: warning non-null emit will be ignored.");
  }
 else {
    this.emit=emit;
  }
  if (transition == null) {
    System.err.println("State: warning null transition vector.");
  }
 else {
    this.transition=transition;
  }
}
