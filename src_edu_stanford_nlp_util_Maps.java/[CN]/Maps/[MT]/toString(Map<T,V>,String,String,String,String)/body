{
  StringBuilder sb=new StringBuilder();
  sb.append(preAppend);
  int i=0;
  for (  Entry<T,V> en : map.entrySet()) {
    if (i != 0)     sb.append(itemSeparator);
    sb.append(en.getKey());
    sb.append(keyValSeparator);
    sb.append(en.getValue());
  }
  sb.append(postAppend);
  return sb.toString();
}
