{
  List<String> features=new ArrayList<String>();
  features.add(FEATURE_PREFIX_WHOLE + WordShapeClassifier.wordShape(mwe,WORD_SHAPER));
  String[] words=mwe.split("_");
  for (  String word : words)   features.add(FEATURE_PREFIX_PART + WordShapeClassifier.wordShape(word,WORD_SHAPER));
  return features;
}
