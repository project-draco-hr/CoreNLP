{
  String[] fields=line.split("\t");
  String gold=fields[0];
  String expression=fields[1];
  List<String> features=new ArrayList<String>();
  for (  Function<String,List<String>> featureFunction : featureFunctions)   features.addAll(featureFunction.apply(expression));
  return new BasicDatum<String,String>(features,gold);
}
