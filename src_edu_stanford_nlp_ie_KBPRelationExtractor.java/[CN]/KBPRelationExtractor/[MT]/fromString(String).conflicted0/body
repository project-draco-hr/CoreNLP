{
  if (name == null || name.equals("")) {
    return Optional.empty();
  }
  name=name.toUpperCase();
  for (  NERTag slot : NERTag.values()) {
    if (slot.name.equals(name))     return Optional.of(slot);
  }
  for (  NERTag slot : NERTag.values()) {
    if (slot.shortName.equals(name))     return Optional.of(slot);
  }
  return Optional.empty();
}
