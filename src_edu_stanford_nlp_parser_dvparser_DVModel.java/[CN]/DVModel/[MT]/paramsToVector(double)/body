{
  int totalSize=totalParamSize();
  if (TRAIN_WORD_VECTORS) {
    return paramsToVector(scale,totalSize,binaryTransform.valueIterator(),unaryTransform.values().iterator(),binaryScore.valueIterator(),unaryScore.values().iterator(),wordVectors.values().iterator());
  }
 else {
    return paramsToVector(scale,totalSize,binaryTransform.valueIterator(),unaryTransform.values().iterator(),binaryScore.valueIterator(),unaryScore.values().iterator());
  }
}
