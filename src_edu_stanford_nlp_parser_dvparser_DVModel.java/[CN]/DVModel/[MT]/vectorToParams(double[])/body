{
  if (TRAIN_WORD_VECTORS) {
    vectorToParams(theta,binaryTransform.valueIterator(),unaryTransform.values().iterator(),binaryScore.valueIterator(),unaryScore.values().iterator(),wordVectors.values().iterator());
  }
 else {
    vectorToParams(theta,binaryTransform.valueIterator(),unaryTransform.values().iterator(),binaryScore.valueIterator(),unaryScore.values().iterator());
  }
}
