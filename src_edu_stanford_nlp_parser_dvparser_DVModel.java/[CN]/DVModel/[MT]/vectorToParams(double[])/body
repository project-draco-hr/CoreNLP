{
  if (op.trainOptions.trainWordVectors) {
    NeuralUtils.vectorToParams(theta,binaryTransform.valueIterator(),unaryTransform.values().iterator(),binaryScore.valueIterator(),unaryScore.values().iterator(),wordVectors.values().iterator());
  }
 else {
    NeuralUtils.vectorToParams(theta,binaryTransform.valueIterator(),unaryTransform.values().iterator(),binaryScore.valueIterator(),unaryScore.values().iterator());
  }
}
