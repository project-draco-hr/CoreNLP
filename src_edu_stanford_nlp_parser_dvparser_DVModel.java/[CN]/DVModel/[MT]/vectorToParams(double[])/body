{
  if (TRAIN_WORD_VECTORS) {
    RNNUtils.vectorToParams(theta,binaryTransform.valueIterator(),unaryTransform.values().iterator(),binaryScore.valueIterator(),unaryScore.values().iterator(),wordVectors.values().iterator());
  }
 else {
    RNNUtils.vectorToParams(theta,binaryTransform.valueIterator(),unaryTransform.values().iterator(),binaryScore.valueIterator(),unaryScore.values().iterator());
  }
}
