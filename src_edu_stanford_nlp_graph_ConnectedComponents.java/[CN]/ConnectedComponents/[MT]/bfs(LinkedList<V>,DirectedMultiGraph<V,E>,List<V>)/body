{
  Set<V> cc=new HashSet<V>();
  while (todo.size() > 0) {
    V node=todo.removeFirst();
    cc.add(node);
    for (    V neighbor : graph.getNeighbors(node)) {
      if (verticesLeft.contains(neighbor)) {
        cc.add(neighbor);
        todo.add(neighbor);
        verticesLeft.remove(neighbor);
      }
    }
  }
  return cc;
}
