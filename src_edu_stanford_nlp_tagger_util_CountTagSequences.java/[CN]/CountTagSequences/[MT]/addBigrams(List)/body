{
  Iterator it=ts.iterator();
  if (!it.hasNext()) {
    System.err.println("Bung!");
    System.exit(0);
  }
  Label t1=(Label)it.next();
  while (it.hasNext()) {
    Label t2=(Label)it.next();
    int i=numb.indexOf(t1,true);
    int j=numb.indexOf(t2,true);
    counts[i][j]++;
    rowcounts[i]++;
    t1=t2;
  }
}
