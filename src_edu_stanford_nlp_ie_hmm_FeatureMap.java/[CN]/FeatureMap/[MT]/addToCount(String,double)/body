{
  if (calculated) {
    System.err.println("Adding after calculating, not good.");
    return;
  }
  FeatureValue fv=f.getValue(s);
  Double val=(Double)counts.get(fv);
  if (val == null) {
    counts.put(fv,new Double(d));
  }
 else {
    counts.put(fv,new Double(val.doubleValue() + d));
  }
  total+=d;
}
