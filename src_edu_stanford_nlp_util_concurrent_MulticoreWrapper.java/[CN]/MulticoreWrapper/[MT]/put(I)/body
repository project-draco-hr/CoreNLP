{
  while (true) {
    if (idleProcessors.peek() == null)     blockingGetResult();
    Integer id=idleProcessors.poll();
    if (id != null) {
      int procId=id;
      CallableJob<I,O> job=new CallableJob<I,O>(item,lastSubmittedItemId++,processorList.get(procId),procId);
      queue.submit(job);
      return;
    }
  }
}
