{
  try {
    ObjectBank<List<CoreLabel>> ob=getObjectBank(new ResettableReaderIteratorFactory(file,flags.inputEncoding));
    if (flags.combo) {
      ((ComboFeatureFactory)featureFactory).dealWithData(ob,file);
    }
    return ob;
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
