{
  System.err.print("Serializing classifier to " + serializePath + "...");
  try {
    ObjectOutputStream oos=IOUtils.writeStreamFromString(serializePath);
    writeObject(oos);
    oos.close();
    System.err.println("done.");
  }
 catch (  Exception e) {
    throw new RuntimeException("Serialization failed",e);
  }
}
