{
  QueriableSequenceModelFactory.QueriableSequenceModelFactoryDataProvider dp;
  if (flags.type.equalsIgnoreCase("cmm")) {
    dp=new CMMFactory();
  }
 else   if (flags.type.equalsIgnoreCase("crf")) {
    dp=new CRFFactory();
  }
 else {
    throw new RuntimeException("Unknown type: " + flags.type);
  }
  return dp;
}
