{
  if (shortName.equals("M"))   return "MASC";
 else   if (shortName.equals("F"))   return "FEM";
 else   if (shortName.equals("S"))   return "SG";
 else   if (shortName.equals("D"))   return "DU";
 else   if (shortName.equals("P"))   return "PL";
 else   throw new RuntimeException(String.format("Bad tag input for (%s)",shortName));
}
