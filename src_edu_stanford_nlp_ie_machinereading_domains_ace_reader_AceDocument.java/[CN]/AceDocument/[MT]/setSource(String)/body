{
  if (p.indexOf("bc/") >= 0)   mSource="broadcast conversation";
 else   if (p.indexOf("bn/") >= 0)   mSource="broadcast news";
 else   if (p.indexOf("cts/") >= 0)   mSource="telephone";
 else   if (p.indexOf("nw/") >= 0)   mSource="newswire";
 else   if (p.indexOf("un/") >= 0)   mSource="usenet";
 else   if (p.indexOf("wl/") >= 0)   mSource="weblog";
 else {
    log.info("WARNING: Unknown source for doc: " + p);
    mSource="none";
  }
}
