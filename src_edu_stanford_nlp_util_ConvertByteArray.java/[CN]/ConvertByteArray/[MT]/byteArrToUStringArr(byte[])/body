{
  int off=0;
  Vector<String> v=new Vector<String>();
  while (off < b.length) {
    int length=byteArrToInt(b,off);
    if (length != 0) {
      v.addElement(byteArrToUString(b,off + 4,length));
    }
 else {
      v.addElement("");
    }
    off=off + 2 * length + 4;
  }
  String[] s=new String[v.size()];
  for (int i=0; i < s.length; i++) {
    s[i]=v.elementAt(i);
  }
  return s;
}
