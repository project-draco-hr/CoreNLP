{
  T existing=get(key);
  if (existing == value || (existing != null && existing.equals(value))) {
    return existing;
  }
 else {
    SoftReference<T> ref=new SoftReference<>(value,this.reaper);
    mapping.put(key,ref);
    if (existing == null) {
      appendBlock(key,value);
    }
 else {
      updateBlockOrDelete(key,value);
    }
    return existing;
  }
}
