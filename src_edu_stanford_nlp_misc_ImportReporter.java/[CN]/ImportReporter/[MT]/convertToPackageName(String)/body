{
  StringBuilder buff=new StringBuilder();
  for (int i=0; i < filename.length(); i++) {
    char c=filename.charAt(i);
    if (c == '/' || c == '\\') {
      c='.';
    }
    buff.append(c);
  }
  int index;
  if ((index=buff.indexOf("edu")) != -1) {
    return buff.substring(index);
  }
 else {
    return "";
  }
}
