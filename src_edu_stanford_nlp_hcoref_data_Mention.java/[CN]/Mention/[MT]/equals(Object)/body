    return false;
  }
  if (!nerString.equals(rhs.nerString)) {
    return false;
  }
  if (startIndex != rhs.startIndex) {
    return false;
  }
  if (endIndex != rhs.endIndex) {
    return false;
  }
  if (headIndex != rhs.headIndex) {
    return false;
  }
  if (mentionID != rhs.mentionID) {
    return false;
  }
  if (originalRef != rhs.originalRef) {
    return false;
  }
  if (!headIndexedWord.equals(rhs.headIndexedWord)) {
    return false;
