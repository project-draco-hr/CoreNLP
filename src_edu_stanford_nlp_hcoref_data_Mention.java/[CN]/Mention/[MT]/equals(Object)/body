{
  if (obj == null) {
    return false;
  }
  if (obj == this) {
    return true;
  }
  if (obj.getClass() != getClass()) {
    return false;
  }
  Mention rhs=(Mention)obj;
  if (!mentionType.equals(rhs.mentionType)) {
    return false;
  }
  if (!number.equals(rhs.number)) {
    return false;
  }
  if (!gender.equals(rhs.gender)) {
    return false;
  }
  if (!animacy.equals(rhs.animacy)) {
    return false;
  }
