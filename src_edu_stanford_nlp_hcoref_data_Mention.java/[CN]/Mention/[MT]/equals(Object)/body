    return false;
  }
  if (isIndirectObject != isIndirectObject) {
    return false;
  }
  if (isPrepositionObject != isPrepositionObject) {
    return false;
  }
  if (hasTwin != rhs.hasTwin) {
    return false;
  }
  if (generic != rhs.generic) {
    return false;
  }
  if (isSingleton != rhs.isSingleton) {
    return false;
  }
  if (!originalSpan.equals(rhs.originalSpan)) {
    return false;
  }
  if (!sentenceWords.equals(rhs.sentenceWords)) {
    return false;
  }
  return true;
}
