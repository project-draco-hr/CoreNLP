    return false;
  }
  if (sentNum != rhs.sentNum) {
    return false;
  }
  if (utter != rhs.utter) {
    return false;
  }
  if (paragraph != rhs.paragraph) {
    return false;
  }
  if (isSubject != rhs.isSubject) {
    return false;
