{
  if (name.equals("head")) {
    targetWordPos=context.size() - 1;
    commitCurrent();
  }
 else   if (name.equals("instance")) {
    instances.add(new SensevalInstance(lexElt,instId,docSource,senseIDs,context,targetWordPos));
  }
 else   if (name.equals("context")) {
    currentString="";
  }
}
