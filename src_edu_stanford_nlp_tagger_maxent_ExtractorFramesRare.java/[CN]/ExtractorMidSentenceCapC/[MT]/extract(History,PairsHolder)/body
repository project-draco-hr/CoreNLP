{
  String prevTag=pH.getTag(h,-1);
  if (prevTag == null) {
    return zeroSt;
  }
  if (prevTag.equals(naTag)) {
    return zeroSt;
  }
  String s=pH.getWord(h,0);
  if (startsUpperCase(s)) {
    String s1=s.toLowerCase();
    if (dict.isUnknown(s1)) {
      return zeroSt;
    }
    return dict.getFirstTag(s1);
  }
  return zeroSt;
}
