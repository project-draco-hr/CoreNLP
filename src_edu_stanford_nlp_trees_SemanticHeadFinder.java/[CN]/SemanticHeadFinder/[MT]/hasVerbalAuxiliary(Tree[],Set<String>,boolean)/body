{
  if (DEBUG) {
    System.err.println("Checking for verbal auxiliary");
  }
  for (  Tree kid : kids) {
    if (DEBUG) {
      System.err.println("  checking in " + kid);
    }
    if (isVerbalAuxiliary(kid,verbalSet,allowTagOnlyMatch)) {
      return true;
    }
  }
  if (DEBUG) {
    System.err.println("hasVerbalAuxiliary returns false");
  }
  return false;
}
