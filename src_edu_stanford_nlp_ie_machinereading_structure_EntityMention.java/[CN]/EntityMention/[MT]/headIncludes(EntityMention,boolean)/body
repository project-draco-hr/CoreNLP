{
  return otherEnt.getSyntacticHeadTokenPosition() >= getHeadTokenStart() && otherEnt.getSyntacticHeadTokenPosition() < getHeadTokenEnd() && ((type != null && otherEnt.type != null && type.equals(otherEnt.type)) || (type == null && otherEnt.type == null)) && (!useSubType || ((subType != null && otherEnt.subType != null && subType.equals(otherEnt.subType)) || (subType == null && otherEnt.subType == null)));
}
