{
  List<ExtractionObject> args=new ArrayList<ExtractionObject>();
  args.add(score);
  args.add(game);
  Span relExtent=new Span(game.getExtent(),score.getExtent());
  RelationMention finalScore=factory.constructRelationMention(RelationMention.makeUniqueId(),game.getSentence(),relExtent,"teamFinalScore",null,args,null);
  setDefaultTypeProbs(finalScore);
  return finalScore;
}
