{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  NFLGame other=(NFLGame)obj;
  if (game == null) {
    if (other.game != null) {
      return false;
    }
  }
 else   if (hashCode() != other.hashCode()) {
    return false;
  }
  return true;
}
