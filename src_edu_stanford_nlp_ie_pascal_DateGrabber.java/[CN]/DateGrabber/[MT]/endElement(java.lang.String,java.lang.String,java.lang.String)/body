{
  if (trainMode) {
    int closingIndex=computeDateIndex(qName);
    if (closingIndex != NUM_TYPES - 1) {
      dateIndex=NUM_TYPES - 1;
    }
    if (qName.equalsIgnoreCase("date")) {
      if (goodDate == 1) {
        numCountedDates++;
      }
      typeCounts[stickyDateIndex][goodDate]++;
    }
  }
}
