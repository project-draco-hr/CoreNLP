{
  if (!Data.matchedTokensForEachPhrase.containsKey(ph))   Data.matchedTokensForEachPhrase.put(ph,new HashMap<String,List<Integer>>());
  Map<String,List<Integer>> matcheds=Data.matchedTokensForEachPhrase.get(ph);
  if (!matcheds.containsKey(sentid))   matcheds.put(sentid,new ArrayList<Integer>());
  for (int i=0; i < length; i++)   matcheds.get(sentid).add(index + i);
}
