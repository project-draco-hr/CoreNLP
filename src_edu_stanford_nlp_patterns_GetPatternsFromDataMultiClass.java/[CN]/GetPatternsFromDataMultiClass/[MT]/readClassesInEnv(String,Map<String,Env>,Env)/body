{
  for (  String line : IOUtils.readLines(s)) {
    String[] toks=line.split("###");
    if (toks.length == 3) {
      String label=toks[0];
      String name=toks[1];
      Class c=Class.forName(toks[2]);
      if (!env.containsKey(label))       env.put(label,TokenSequencePattern.getNewEnv());
      env.get(label).bind(name,c);
    }
 else     if (toks.length == 2) {
      String name=toks[0];
      Class c=Class.forName(toks[1]);
      globalEnv.bind(name,c);
    }
 else     throw new RuntimeException("Ill formed env file!");
  }
}
