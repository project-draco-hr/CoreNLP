  Map<String,Set<String>> seedSets=new HashMap<String,Set<String>>();
  seedSets.put(answerLabel,seedSet);
  setUpConstructor(sents,seedSets,labelUsingSeedSets,ansCl,generalizeClasses,ignoreClasses);
