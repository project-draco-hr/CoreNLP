{
  List<String> ret=new ArrayList<String>(sentence.size());
  for (  CoreLabel l : sentence) {
    ret.add(String.format("%d\t%s\t%s\t%s\t%s\t%s\t%d\t%s\t_\t_",l.index(),l.word(),l.lemma(),l.tag(),l.get(CoreAnnotations.CoarseTagAnnotation.class),l.get(CoreAnnotations.ValueAnnotation.class),l.get(CoreAnnotations.CoNLLDepParentIndexAnnotation.class),l.get(CoreAnnotations.CoNLLDepTypeAnnotation.class)));
  }
  return ret;
}
