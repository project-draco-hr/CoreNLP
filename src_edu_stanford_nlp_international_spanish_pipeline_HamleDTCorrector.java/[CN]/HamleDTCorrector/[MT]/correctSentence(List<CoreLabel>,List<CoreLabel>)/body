{
  List<CoreLabel> ret=new ArrayList<CoreLabel>(hamledtSentence.size());
  CoreLabel hamWord=null, ancWord=null;
  for (Iterator<CoreLabel> iHam=hamledtSentence.iterator(), iAnc=ancoraSentence.iterator(); iHam.hasNext() && iAnc.hasNext(); hamWord=iHam.next(), ancWord=iAnc.next()) {
    ret.add(correctWord(hamWord,ancWord));
  }
  return ret;
}
