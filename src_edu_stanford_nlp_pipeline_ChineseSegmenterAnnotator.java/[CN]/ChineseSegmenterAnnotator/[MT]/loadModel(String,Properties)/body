{
  if (VERBOSE) {
    timer.start();
    System.err.print("Loading Segmentation Model [" + segLoc + "]...");
  }
  try {
    segmenter=CRFClassifier.getClassifier(segLoc,props);
  }
 catch (  RuntimeException e) {
    throw e;
  }
catch (  Exception e) {
    throw new RuntimeException(e);
  }
  if (VERBOSE) {
    timer.stop("done.");
  }
}
