{
  int x;
  if (position1 > position2) {
    x=position2;
    position2=position1;
    position1=x;
  }
  if (position2 > position3) {
    x=position3;
    position3=position2;
    position2=x;
  }
  if (position1 > position2) {
    x=position2;
    position2=position1;
    position1=x;
  }
  this.position1=position1;
  this.position2=position2;
  this.position3=position3;
}
