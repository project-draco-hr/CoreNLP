{
  int correct=0;
  int total=0;
  for (; exampleIterator.hasNext(); ) {
    RVFDatum<L,F> next=exampleIterator.next();
    L guess=classOf(next);
    if (guess.equals(next.label())) {
      correct++;
    }
    total++;
  }
  Logging.logger(this.getClass()).info("correct " + correct + " out of "+ total);
  return correct / (float)total;
}
