{
  sentRight=0;
  sentWrong=0;
  tagRight=0;
  tagWrong=0;
  unknownTagRight=0;
  unknownTagWrong=0;
  Iterator<ArrayList<TaggedWord>> i2=sb2.iterator();
  for (  List<TaggedWord> s1 : sb1) {
    if (!i2.hasNext()) {
      throw new RuntimeException("Sentencebank size mismatch!");
    }
    List<TaggedWord> s2=i2.next();
    evaluateSentence(s1,s2);
  }
}
