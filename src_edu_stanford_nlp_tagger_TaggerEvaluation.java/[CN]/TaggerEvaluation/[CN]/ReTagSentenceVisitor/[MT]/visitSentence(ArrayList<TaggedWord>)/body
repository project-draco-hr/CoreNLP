{
  int leng=s.size();
  for (int i=0; i < leng; i++) {
    TaggedWord tw=s.get(i);
    String tag=tw.tag();
    String word=tw.word();
    if (word.equals("#")) {
      tag="#";
    }
 else     if (word.equalsIgnoreCase("to") && tag.equals("IN")) {
      tag="TO";
    }
 else     if (tag.equals("DTP")) {
      tag="DT";
    }
 else     if (tag.equals("INS")) {
      tag="IN";
    }
    TaggedWord tw2=new TaggedWord(word,tag);
    s.set(i,tw2);
  }
}
