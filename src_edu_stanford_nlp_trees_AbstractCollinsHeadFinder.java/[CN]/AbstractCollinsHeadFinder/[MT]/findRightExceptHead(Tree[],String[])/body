{
  for (int headIdx=daughterTrees.length - 1; headIdx >= 0; headIdx--) {
    String childCat=tlp.basicCategory(daughterTrees[headIdx].label().value());
    boolean found=true;
    for (int i=1; i < how.length; i++) {
      if (how[i].equals(childCat)) {
        found=false;
      }
    }
    if (found) {
      return headIdx;
    }
  }
  return -1;
}
