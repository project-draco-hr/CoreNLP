{
  ShiftReduceParser parser=null;
  try {
    parser=IOUtils.readObjectFromFile(path);
  }
 catch (  IOException e) {
    throw new RuntimeIOException(e);
  }
catch (  ClassNotFoundException e) {
    throw new RuntimeIOException(e);
  }
  if (extraFlags.length > 0) {
    parser.setOptionFlags(extraFlags);
  }
  return parser;
}
