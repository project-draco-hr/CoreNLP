{
  if (nThreads == 1) {
    for (    Tree tree : trees) {
      redoTags(tree,tagger);
    }
  }
 else {
    MulticoreWrapper<Tree,Tree> wrapper=new MulticoreWrapper<Tree,Tree>(nThreads,new RetagProcessor(tagger));
    for (    Tree tree : trees) {
      wrapper.put(tree);
    }
    wrapper.join();
  }
}
