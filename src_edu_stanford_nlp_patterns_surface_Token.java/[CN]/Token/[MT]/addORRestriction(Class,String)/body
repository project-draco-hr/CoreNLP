{
  getKeyForClass(classR);
  if (this.envBindBooleanRestriction != null && !this.envBindBooleanRestriction.isEmpty())   throw new RuntimeException("cannot add restriction to something that is binding to an env variable");
  if (classORrestrictions == null)   classORrestrictions=new TreeMap<Class,String>(new ClassComparator());
  classORrestrictions.put(classR,value);
}
