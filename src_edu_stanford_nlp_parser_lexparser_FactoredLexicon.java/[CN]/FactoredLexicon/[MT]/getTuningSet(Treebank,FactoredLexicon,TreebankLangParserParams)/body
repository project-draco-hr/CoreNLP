{
  List<Tree> devTrees=new ArrayList<Tree>(3000);
  for (  Tree tree : devTreebank) {
    for (    Tree subTree : tree) {
      if (!subTree.isLeaf()) {
        tlpp.transformTree(subTree,tree);
      }
    }
    devTrees.add(tree);
  }
  List<FactoredLexiconEvent> tuningSet=treebankToLexiconEvents(devTrees,lexicon);
  return tuningSet;
}
