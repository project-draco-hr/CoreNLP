{
  ConfigParser cp=new ConfigParser("configurations/sample.conf");
  cp.parse();
  System.out.println(cp.toString());
  for (  Properties sm : cp) {
    System.out.println("--------------------");
    for (    String key : sm.stringPropertyNames())     System.out.printf(" %s: %s\n",key,sm.get(key));
  }
}
