{
  List<String> rtn=new ArrayList<>();
  for (  SemanticGraphEdge edge : semanticGraphEdges) {
    rtn.add(edge.getRelation().getShortName());
  }
  while (state != null) {
    if (state.lastDeletedEdge != null) {
      rtn.add(state.lastDeletedEdge);
    }
    state=state.source;
  }
  return rtn;
}
