{
  Tree[] kids=children();
  List<Tree> l=new ArrayList<Tree>();
  for (  Tree kid : kids) {
    l.addAll(kid.spliceOutHelper(nodeFilter,tf));
  }
  if (nodeFilter.accept(this)) {
    Tree t;
    if (!l.isEmpty()) {
      t=tf.newTreeNode(label(),l);
    }
 else {
      t=tf.newLeaf(label());
    }
    l=new ArrayList<Tree>(1);
    l.add(t);
    return l;
  }
  return l;
}
