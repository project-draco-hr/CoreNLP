{
  Tree t;
  if (isLeaf()) {
    t=tf.newLeaf(label());
  }
 else {
    Tree[] kids=children();
    List<Tree> newKids=new ArrayList<Tree>(kids.length);
    for (int i=0, n=kids.length; i < n; i++) {
      newKids.add(kids[i].treeSkeletonCopy(tf));
    }
    t=tf.newTreeNode(label(),newKids);
  }
  return t;
}
