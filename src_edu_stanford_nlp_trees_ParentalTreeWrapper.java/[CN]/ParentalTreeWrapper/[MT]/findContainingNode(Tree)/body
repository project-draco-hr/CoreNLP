{
  if (this == node || this.tree == node) {
    return this;
  }
  for (  Tree child : children) {
    if (!(child instanceof ParentalTreeWrapper)) {
      throw new AssertionError();
    }
    Tree result=((ParentalTreeWrapper)child).findContainingNode(node);
    if (result != null) {
      return result;
    }
  }
  return null;
}
