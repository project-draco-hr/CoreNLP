{
  if (s1 == null) {
    if (s2 == null || s2.length() == 0) {
      return true;
    }
 else {
      return false;
    }
  }
 else   if (s2 == null) {
    if (s1 == null || s1.length() == 0) {
      return true;
    }
 else {
      return false;
    }
  }
 else   if (s1.equals(s2)) {
    return true;
  }
 else {
    return false;
  }
}
