{
  if (tagsChanged) {
    int n=JOptionPane.showConfirmDialog(this,"Save changes to current tag file first?","Opening",JOptionPane.YES_NO_CANCEL_OPTION);
    if (n == JOptionPane.YES_OPTION) {
      if (!saveTags(true)) {
        return false;
      }
    }
 else     if (n == JOptionPane.CANCEL_OPTION) {
      return false;
    }
  }
  return true;
}
