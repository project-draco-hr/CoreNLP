{
  WordToSentenceProcessor<CoreLabel> wts;
  if (nlToken.length == 0) {
    wts=new WordToSentenceProcessor<CoreLabel>("",Collections.<String>emptySet(),Collections.<String>emptySet());
  }
 else   if (nlToken.length == 1) {
    wts=new WordToSentenceProcessor<CoreLabel>("",Collections.<String>emptySet(),Collections.singleton(nlToken[0]));
  }
 else {
    Set<String> nlTokens=new HashSet<String>(Arrays.asList(nlToken));
    wts=new WordToSentenceProcessor<CoreLabel>("",Collections.<String>emptySet(),nlTokens);
  }
  return new WordsToSentencesAnnotator(wts,verbose);
}
