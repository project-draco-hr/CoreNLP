{
  while (keepGoing()) {
    Job j;
synchronized (jobQueue) {
      while (jobQueue.isEmpty() && keepGoing()) {
        try {
          jobQueue.wait();
        }
 catch (        InterruptedException e) {
        }
      }
      if (!keepGoing()) {
        break;
      }
      j=(Job)jobQueue.removeFirst();
      jobQueue.notifyAll();
    }
    j.run();
    j=null;
  }
}
