{
  StringBuilder sb=new StringBuilder();
  for (int j=left; j <= right; j++) {
    String word=pH.getWord(h,j);
    String distSim=lexicon.get(word.toLowerCase());
    if (distSim == null)     distSim="null";
    sb.append(distSim);
    if (j < right) {
      sb.append('|');
    }
  }
  return sb.toString();
}
