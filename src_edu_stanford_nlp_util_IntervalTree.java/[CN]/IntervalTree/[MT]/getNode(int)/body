{
  int i=nodeIndex;
  IntervalTree<E,T> n=this;
  while (n != null) {
    if (i < 0 || i >= n.size)     return null;
    int leftSize=(n.left != null) ? n.left.size : 0;
    if (i == leftSize) {
      return n;
    }
 else     if (i > leftSize) {
      n=n.right;
      i=i - leftSize - 1;
    }
 else {
      n=n.left;
    }
  }
  return null;
}
