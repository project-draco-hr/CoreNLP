{
  return tree.prune(new Filter<Tree>(){
    /** 
 */
    private static final long serialVersionUID=722395824663294192L;
    public boolean accept(    Tree t){
      Tree[] kids=t.children();
      if ((t.label() != null) && !t.isLeaf() && kids.length == 1 && kids[0].isLeaf() && t.label().value() != null && (t.label().value().equals("-NONE-") || tlp.isPunctuationTag(t.label().value()))) {
        return false;
      }
      return true;
    }
  }
,tf);
}
