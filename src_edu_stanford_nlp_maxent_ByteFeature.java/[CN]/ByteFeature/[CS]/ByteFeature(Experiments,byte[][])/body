{
  domain=e;
  int num=0;
  for (int x=0; x < e.xSize; x++) {
    for (int y=0; y < e.ySize; y++) {
      if (vals[x][y] != 0) {
        num++;
      }
    }
  }
  indexedValues=new int[num];
  valuesI=new byte[num];
  int current=0;
  for (int x=0; x < e.xSize; x++) {
    for (int y=0; y < e.ySize; y++) {
      if (vals[x][y] != 0) {
        indexedValues[current]=x * e.ySize + y;
        valuesI[current]=vals[x][y];
        current++;
      }
    }
  }
}
