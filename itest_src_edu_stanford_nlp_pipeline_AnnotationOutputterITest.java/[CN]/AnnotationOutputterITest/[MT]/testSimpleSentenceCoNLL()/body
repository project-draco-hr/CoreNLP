{
  Annotation ann=new Annotation("The cat is fat. The dog is lazy.");
  pipeline.annotate(ann);
  String actual=new CoNLLOutputter().print(ann);
  String expected="1\tThe\tthe\tDT\tO\t2\tdet\n" + "2\tcat\tcat\tNN\tO\t4\tnsubj\n" + "3\tis\tbe\tVBZ\tO\t4\tcop\n"+ "4\tfat\tfat\tJJ\tO\t0\tROOT\n"+ "5\t.\t.\t.\tO\t_\t_\n"+ "\n"+ "1\tThe\tthe\tDT\tO\t2\tdet\n"+ "2\tdog\tdog\tNN\tO\t4\tnsubj\n"+ "3\tis\tbe\tVBZ\tO\t4\tcop\n"+ "4\tlazy\tlazy\tJJ\tO\t0\tROOT\n"+ "5\t.\t.\t.\tO\t_\t_\n"+ "\n";
  assertEquals(expected,actual);
}
