{
  String stripped=stripSuffix(verb,pOneAttachedPronoun);
  if (stripped == null || (!isVerb(stripped) && !isVerb(stripped + "d"))) {
    System.out.println(stripped);
    stripped=stripSuffix(verb,pTwoAttachedPronouns);
    if (stripped == null || (!isVerb(stripped) && !isVerb(stripped + "d"))) {
      System.out.println(stripped);
      return null;
    }
  }
  return stripped;
}
