{
  int g=year % 19;
  int c=year / 100;
  int h=(c - (c / 4) - ((8 * c + 13) / 25) + 19 * g + 15) % 30;
  int i=h - (h / 28) * (1 - (h / 28) * (29 / (h + 1)) * ((21 - g) / 11));
  int j=(year + (year / 4) + i+ 2 - c + (c / 4)) % 7;
  int l=i - j;
  int m=3 + ((l + 40) / 44);
  int d=l + 28 - 31 * (m / 4);
  String date=String.format("%04d%02d%02d",year,m,d);
  return (date);
}
