{
  if (datum instanceof RVFDatum)   return justificationOfRVFDatum((RVFDatum<Double,F>)datum);
  Counter<F> featureWeights=new ClassicCounter<F>();
  for (  F feature : datum.asFeatures()) {
    int fID=featureIndex.indexOf(feature);
    if (fID >= 0)     featureWeights.incrementCount(feature,weights[fID]);
  }
  return featureWeights;
}
