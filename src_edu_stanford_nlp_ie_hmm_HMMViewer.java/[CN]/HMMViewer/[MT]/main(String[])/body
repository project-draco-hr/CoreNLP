{
  if (args.length == 0) {
    System.err.println("Usage: java HMMViewer hmmfile");
    System.exit(1);
  }
  File hmmFile=new File(args[0]);
  if (!hmmFile.canRead()) {
    System.err.println("ERROR: Cannot read " + hmmFile);
    System.exit(1);
  }
  try {
    HMM hmm=(HMM)new ObjectInputStream(new FileInputStream(hmmFile)).readObject();
    hmm.printProbs();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
