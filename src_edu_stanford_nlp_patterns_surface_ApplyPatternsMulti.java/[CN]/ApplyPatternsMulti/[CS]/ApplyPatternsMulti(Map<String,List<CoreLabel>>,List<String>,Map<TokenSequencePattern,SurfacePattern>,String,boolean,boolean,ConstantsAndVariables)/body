{
  this.sents=sents;
  this.patterns=patterns;
  multiPatternMatcher=TokenSequencePattern.getMultiPatternMatcher(patterns.keySet());
  this.sentids=sentids;
  this.label=label;
  this.removeStopWordsFromSelectedPhrases=removeStopWordsFromSelectedPhrases;
