{
  if (qName.equals("tag")) {
    String tag=attrs.getValue("name");
    String val=attrs.getValue("value");
    if (val.equals("start")) {
      addPreviousToDoc(otherTag);
    }
 else     if (val.equals("end")) {
      addPreviousToDoc(tag);
    }
 else {
      throw new RuntimeException("unexpected value=" + val);
    }
  }
}
