{
  cl.remove(ParentAnnotation.class);
  String word=cl.word();
  String first;
  String second;
  String lowered=word.toLowerCase();
  if (lowered.equals("del") || lowered.equals("al")) {
    first=lowered.substring(0,lowered.length() - 1);
    char lastChar=lowered.charAt(lowered.length() - 1);
    if (Character.isLowerCase(lastChar))     second="el";
 else     second="EL";
  }
 else   if (lowered.equals("conmigo") || lowered.equals("consigo")) {
    first=lowered.substring(0,3);
    second=lowered.charAt(3) + "??";
  }
 else   if (lowered.equals("contigo")) {
    first="con";
    second="ti";
  }
 else {
    return null;
  }
  compoundBuffer.add(copyCoreLabel(cl,second));
  return copyCoreLabel(cl,first);
}
