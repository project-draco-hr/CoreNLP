{
  Map<String,String> macros=new HashMap<String,String>();
  for (String line; (line=reader.readLine()) != null; ) {
    line=line.trim();
    if (line.startsWith("macro ")) {
      Pair<String,String> macro=extractMacro(line);
      macros.put(macro.first(),macro.second());
    }
  }
  return macros;
}
