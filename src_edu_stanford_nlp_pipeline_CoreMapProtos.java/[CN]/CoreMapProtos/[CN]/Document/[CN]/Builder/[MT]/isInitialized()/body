{
  if (!hasText()) {
    return false;
  }
  for (int i=0; i < getTokenCount(); i++) {
    if (!getToken(i).isInitialized()) {
      return false;
    }
  }
  for (int i=0; i < getSentenceCount(); i++) {
    if (!getSentence(i).isInitialized()) {
      return false;
    }
  }
  for (int i=0; i < getCorefChainCount(); i++) {
    if (!getCorefChain(i).isInitialized()) {
      return false;
    }
  }
  return true;
}
