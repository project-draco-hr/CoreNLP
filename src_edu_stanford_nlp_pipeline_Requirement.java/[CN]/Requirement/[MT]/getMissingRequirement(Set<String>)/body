{
  for (  List<String> requirement : requirements) {
    boolean found=false;
    for (    String annotator : requirement) {
      if (alreadyAdded.contains(annotator)) {
        found=true;
        break;
      }
    }
    if (!found) {
      return StringUtils.join(requirement,"|");
    }
  }
  return null;
}
