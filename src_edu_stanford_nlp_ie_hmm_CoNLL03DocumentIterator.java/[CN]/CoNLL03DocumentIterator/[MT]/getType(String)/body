{
  if (typeLabel.equals("O")) {
    return (0);
  }
  int hyphenIndex=typeLabel.indexOf('-');
  if (hyphenIndex != -1) {
    typeLabel=typeLabel.substring(hyphenIndex + 1);
  }
  for (int i=1; i < targetFields.length; i++) {
    if (typeLabel.equals(targetFields[i])) {
      return (i);
    }
  }
  return (-1);
}
