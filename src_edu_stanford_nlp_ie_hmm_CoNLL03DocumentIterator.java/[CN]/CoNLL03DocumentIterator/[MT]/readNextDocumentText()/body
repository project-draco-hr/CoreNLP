{
  StringBuffer docBuffer=new StringBuffer();
  String line;
  boolean started=false;
  try {
    while ((line=br.readLine()) != null) {
      if (line.startsWith(DOCSTART) || line.startsWith("======")) {
        if (started) {
          break;
        }
 else {
          continue;
        }
      }
      started=true;
      docBuffer.append(line);
      docBuffer.append('\n');
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  if (docBuffer.length() == 0) {
    return (null);
  }
  return (docBuffer.toString());
}
