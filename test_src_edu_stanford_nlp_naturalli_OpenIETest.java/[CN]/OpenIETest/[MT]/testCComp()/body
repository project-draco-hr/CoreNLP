{
  assertEquals(new HashSet<String>(){
{
      add("I suggested that he leave the room");
      add("he leave the room");
    }
  }
,clauses("1\tI\t2\tnsubj\tPR\n" + "2\tsuggested\t0\troot\tVBD\n" + "3\tthat\t5\tmark\tIN\n"+ "4\the\t5\tnsubj\tPR\n"+ "5\tleave\t2\tccomp\tVB\n"+ "6\tthe\t7\tdet\tDT\n"+ "7\troom\t5\tdobj\tNN\n"));
}
