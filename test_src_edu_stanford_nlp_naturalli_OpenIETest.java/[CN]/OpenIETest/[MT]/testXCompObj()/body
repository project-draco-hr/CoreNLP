{
  assertEquals(new HashSet<String>(){
{
      add("I persuaded Fred to leave the room");
      add("Fred to leave the room");
    }
  }
,clauses("1\tI\t2\tnsubj\n" + "2\tpersuaded\t0\troot\n" + "3\tFred\t2\tdobj\n"+ "4\tto\t5\taux\n"+ "5\tleave\t2\txcomp\n"+ "6\tthe\t7\tdet\n"+ "7\troom\t5\tdobj\n"));
}
