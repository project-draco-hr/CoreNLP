{
  assertEquals(new HashSet<String>(){
{
      add("I was persuaded to leave the room");
      add("I leave the room");
    }
  }
,clauses("1\tI\t3\tnsubjpass\n" + "2\twas\t3\tauxpass\n" + "3\tpersuaded\t0\troot\n"+ "4\tto\t5\taux\n"+ "5\tleave\t3\txcomp\n"+ "6\tthe\t7\tdet\n"+ "7\troom\t5\tdobj\n"));
}
