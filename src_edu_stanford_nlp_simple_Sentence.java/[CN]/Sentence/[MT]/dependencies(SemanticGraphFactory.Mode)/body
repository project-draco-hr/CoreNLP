{
switch (mode) {
case COLLAPSED_TREE:
    return impl.getCollapsedDependencies();
case COLLAPSED:
  return impl.getCollapsedDependencies();
case CCPROCESSED:
return impl.getCollapsedCCProcessedDependencies();
case BASIC:
return impl.getBasicDependencies();
default :
throw new IllegalArgumentException("Unknown dependency type: " + mode);
}
}
