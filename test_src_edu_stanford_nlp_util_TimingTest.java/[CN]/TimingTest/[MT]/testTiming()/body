{
  Timing t=new Timing();
  sleepTen();
  long val2=t.reportNano();
  assertEquals("Wrong nanosleep",10000000,val2,5000000);
  sleepTen();
  long val=t.report();
  assertEquals("Wrong sleep",20,val,4);
  for (int i=0; i < 8; i++) {
    sleepTen();
  }
  long val3=t.report();
  assertEquals("Wrong formatted time","0.1",Timing.toSecondsString(val3));
}
