{
  if (PRINT_RUNNING_SCORES) {
    System.err.println("getScore(" + line + ","+ category+ ")");
  }
  double score=Math.log(getPriorProb(category));
  if (usePriorBoost) {
    score*=priorBoost;
  }
  if (PRINT_RUNNING_SCORES) {
    System.err.println("prior: " + score);
  }
  return (score + getLogProb(line,category));
}
