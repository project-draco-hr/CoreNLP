{
  boolean hasSegmentationMarkers=true;
  boolean hasTags=true;
  DocumentReaderAndWriter<CoreLabel> docReader=new ArabicDocumentReaderAndWriter(hasSegmentationMarkers,hasTags,hasDomainLabels,tf);
  ObjectBank<List<CoreLabel>> lines=classifier.makeObjectBankFromFile(flags.trainFile,docReader);
  classifier.train(lines,docReader);
  System.err.println("Finished training.");
}
