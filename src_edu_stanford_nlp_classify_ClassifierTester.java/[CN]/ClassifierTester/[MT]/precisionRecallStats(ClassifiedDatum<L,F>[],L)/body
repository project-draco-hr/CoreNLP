{
  PrecisionRecallStats stats=new PrecisionRecallStats();
  for (int i=0; i < results.length; i++) {
    if (results[i].label().equals(label)) {
      if (results[i].isCorrect()) {
        stats.incrementTP();
      }
 else {
        stats.incrementFN();
      }
    }
 else     if (results[i].getPredictedLabel().equals(label)) {
      stats.incrementFP();
    }
  }
  return (stats);
}
