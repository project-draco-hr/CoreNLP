{
  if (label == null) {
    return "";
  }
  boolean nptemp=NPTmpPattern.matcher(label).matches();
  boolean npadv=NPAdvPattern.matcher(label).matches();
  label=tlp.basicCategory(label);
  if (nptemp) {
    label=label + "-TMP";
  }
 else   if (npadv) {
    label=label + "-ADV";
  }
  return label;
}
