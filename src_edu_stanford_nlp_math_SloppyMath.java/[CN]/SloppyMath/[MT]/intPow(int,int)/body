{
  if (e <= 1) {
    if (e == 1) {
      return b;
    }
 else {
      return 0;
    }
  }
 else {
    if (e == 2) {
      return b * b;
    }
 else {
      int result=1;
      while (e > 0) {
        if ((e & 1) != 0) {
          result*=b;
        }
        b*=b;
        e>>=1;
      }
      return result;
    }
  }
}
