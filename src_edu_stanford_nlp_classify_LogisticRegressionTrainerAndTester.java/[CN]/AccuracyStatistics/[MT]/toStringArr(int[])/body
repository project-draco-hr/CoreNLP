{
  StringBuilder sb=new StringBuilder();
  int total=acc.length;
  for (int i=0; i < acc.length; i++) {
    double coverage=(i + 1) / (double)total;
    double accuracy=acc[i] / (double)(i + 1);
    coverage*=1000000;
    accuracy*=1000000;
    coverage=(int)coverage;
    accuracy=(int)accuracy;
    coverage/=10000;
    accuracy/=10000;
    sb.append(coverage);
    sb.append("\t");
    sb.append(accuracy);
    sb.append("\n");
  }
  return sb.toString();
}
