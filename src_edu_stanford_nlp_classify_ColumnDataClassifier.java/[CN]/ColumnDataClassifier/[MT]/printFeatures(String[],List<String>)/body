{
  if (cliqueWriter != null) {
    for (int i=0; i < wi.length; i++) {
      if (i > 0) {
        cliqueWriter.print("\t");
      }
      cliqueWriter.print(wi[i]);
    }
    for (    String feat : features) {
      cliqueWriter.print("\t");
      cliqueWriter.print(feat);
    }
    cliqueWriter.println();
  }
}
