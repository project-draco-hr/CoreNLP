{
  int numCoSup=0;
  if (directionSensitive) {
    for (    V parent : graph.getParents(node1)) {
      if (!parent.equals(node2) && graph.getChildren(parent).contains(node2))       numCoSup++;
    }
  }
 else {
    for (    V parent : graph.getNeighbors(node1)) {
      if (!parent.equals(node2) && graph.getNeighbors(parent).contains(node2))       numCoSup++;
    }
  }
  return numCoSup;
}
