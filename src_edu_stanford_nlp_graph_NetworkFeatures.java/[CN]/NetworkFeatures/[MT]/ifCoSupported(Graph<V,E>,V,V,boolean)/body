{
  if (graph.isEdge(node1,node2))   return false;
  if (directionSensitive) {
    for (    V parent : graph.getParents(node1)) {
      if (!parent.equals(node2) && graph.getChildren(parent).contains(node2))       return true;
    }
  }
 else {
    for (    V parent : graph.getNeighbors(node1)) {
      if (!parent.equals(node2) && graph.getNeighbors(parent).contains(node2))       return true;
    }
  }
  return false;
}
