{
  String text=currentText.toString().trim();
  if (text.length() >= 0) {
    String[] pieces=text.split(" +");
    for (    String word : pieces) {
      word=word.trim();
      if (word.equals("")) {
        continue;
      }
      words.add(word);
      tags.add(inside == null ? "O" : inside);
    }
  }
  currentText=new StringBuilder();
}
