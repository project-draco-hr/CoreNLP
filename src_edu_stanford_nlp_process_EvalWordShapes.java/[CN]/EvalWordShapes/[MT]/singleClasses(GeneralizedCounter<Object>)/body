{
  int count=0;
  Iterator<Object> it=gc.topLevelKeySet().iterator();
  String wordshape;
  while (it.hasNext()) {
    wordshape=(String)it.next();
    List<Object> l=new ArrayList<Object>(1);
    l.add(wordshape);
    GeneralizedCounter<Object> c=gc.conditionalize(l);
    if (c.totalCount() == 1.0) {
      count++;
    }
    ;
  }
  return count;
}
