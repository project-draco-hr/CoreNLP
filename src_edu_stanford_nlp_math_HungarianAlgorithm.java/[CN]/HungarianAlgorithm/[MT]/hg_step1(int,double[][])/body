{
  double minval;
  for (int i=0; i < cost.length; i++) {
    minval=cost[i][0];
    for (int j=0; j < cost[i].length; j++) {
      if (minval > cost[i][j]) {
        minval=cost[i][j];
      }
    }
    for (int j=0; j < cost[i].length; j++) {
      cost[i][j]=cost[i][j] - minval;
    }
  }
  step=2;
  return step;
}
