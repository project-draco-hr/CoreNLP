{
  for (int i=0; i < cost.length; i++) {
    for (int j=0; j < cost[i].length; j++) {
      if ((cost[i][j] == 0) && (colCover[j] == 0) && (rowCover[i] == 0)) {
        mask[i][j]=1;
        colCover[j]=1;
        rowCover[i]=1;
      }
    }
  }
  clearCovers(rowCover,colCover);
  step=3;
  return step;
}
