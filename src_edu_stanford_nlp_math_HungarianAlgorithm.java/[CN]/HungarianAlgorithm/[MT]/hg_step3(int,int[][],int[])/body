{
  for (int i=0; i < mask.length; i++) {
    for (int j=0; j < mask[i].length; j++) {
      if (mask[i][j] == 1) {
        colCover[j]=1;
      }
    }
  }
  int count=0;
  for (int j=0; j < colCover.length; j++) {
    count=count + colCover[j];
  }
  if (count >= mask.length) {
    step=7;
  }
 else {
    step=4;
  }
  return step;
}
