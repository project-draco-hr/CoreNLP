{
  double minval=findSmallest(cost,rowCover,colCover,maxCost);
  for (int i=0; i < rowCover.length; i++) {
    for (int j=0; j < colCover.length; j++) {
      if (rowCover[i] == 1) {
        cost[i][j]=cost[i][j] + minval;
      }
      if (colCover[j] == 0) {
        cost[i][j]=cost[i][j] - minval;
      }
    }
  }
  step=4;
  return step;
}
