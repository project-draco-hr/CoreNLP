{
  if (this == o) {
    return true;
  }
  if (!(o instanceof Instance)) {
    return false;
  }
  final Instance instance=(Instance)o;
  if (index != instance.index) {
    return false;
  }
  if (!sentence.equals(instance.sentence)) {
    return false;
  }
  if (!word.equals(instance.word)) {
    return false;
  }
  return true;
}
