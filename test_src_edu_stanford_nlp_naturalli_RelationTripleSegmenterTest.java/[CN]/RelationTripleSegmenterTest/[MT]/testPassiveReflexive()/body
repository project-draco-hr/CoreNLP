{
  Optional<RelationTriple> extraction=mkExtraction("1\tTom\t4\tnsubjpass\n" + "2\tJerry\t1\tconj:and\n" + "3\twere\t4\tauxpass\n"+ "4\tfighting\t0\troot\n");
  assertTrue("No extraction for sentence!",extraction.isPresent());
  assertEquals("1.0\tTom\tfighting\tJerry",extraction.get().toString());
}
