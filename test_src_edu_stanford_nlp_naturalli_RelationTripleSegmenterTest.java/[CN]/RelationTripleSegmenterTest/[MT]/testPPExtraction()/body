{
  Optional<RelationTriple> extraction=mkExtraction("1\tObama\t0\troot\tNNP\tPERSON\n" + "2\tTucson\t1\tnmod:in\tNNP\tLOCATION\n");
  assertTrue("No extraction for sentence!",extraction.isPresent());
  assertEquals("1.0\tObama\tis in\tTucson",extraction.get().toString());
  extraction=mkExtraction("1\tPietro\t2\tcompound\tNNP\tPERSON\n" + "2\tBadoglio\t0\troot\tNNP\tPERSON\n" + "3\tsouthern\t4\tamod\tJJ\tO\n"+ "4\tItaly\t2\tnmod:in\tNN\tLOCATION\n");
  assertTrue("No extraction for sentence!",extraction.isPresent());
  assertEquals("1.0\tPietro Badoglio\tis in\tItaly",extraction.get().toString());
}
