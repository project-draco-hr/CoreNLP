{
  Optional<RelationTriple> extraction=mkExtraction("1\tthere\t2\texpl\n" + "2\tare\t0\troot\tVBP\tO\tbe\n" + "3\tdogs\t2\tnsubj\tNN\n"+ "4\theaven\t3\tnmod:in\tNN\n",true);
  assertTrue("No extraction for sentence!",extraction.isPresent());
  assertEquals("1.0\tdogs\tis in\theaven",extraction.get().toString());
}
