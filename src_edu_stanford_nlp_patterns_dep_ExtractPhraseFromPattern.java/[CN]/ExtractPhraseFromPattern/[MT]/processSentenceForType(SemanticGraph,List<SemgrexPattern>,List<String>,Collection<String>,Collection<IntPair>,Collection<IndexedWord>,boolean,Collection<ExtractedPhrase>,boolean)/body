{
  for (  SemgrexPattern pattern : typePatterns) {
    Collection<IndexedWord> triggerWords=getSemGrexPatternNodes(g,textTokens,typePhrases,typeIndices,pattern,findSubTrees,extractedPhrases,lowercase);
    for (    IndexedWord w : triggerWords) {
      if (!typeTriggerWords.contains(w))       typeTriggerWords.add(w);
    }
  }
}
