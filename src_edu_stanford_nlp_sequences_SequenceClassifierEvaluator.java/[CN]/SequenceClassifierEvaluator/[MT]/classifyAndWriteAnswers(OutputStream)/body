{
  PrintWriter pw=new PrintWriter(outputStream);
  for (  CliqueDataset c : dataset.getDatasets()) {
    modelFactory.test(c);
    for (    CoreLabel label : c.sourceDoc) {
      pw.println(label.word() + "\t" + label.get(CoreAnnotations.GoldAnswerAnnotation.class)+ "\t"+ label.get(CoreAnnotations.AnswerAnnotation.class));
    }
    pw.println();
  }
  pw.flush();
}
