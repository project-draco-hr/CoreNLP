{
  Iterator<Tree> itr=span.iterator();
  while (itr.hasNext()) {
    Tree leaf=itr.next();
    int index=(int)leaf.score();
    if (index >= labels.size())     System.err.printf("ERROR: Out of bounds index %d at leaf\n",index);
 else     if (newLabel.matches(".*NP_SUBJ.*") && labels.get(index).matches(".*NP_SUBJ.*"))     System.err.printf("Skipping recursive NP-SBJ at index %d\n",index);
 else     labels.set(index,new String(newLabel));
  }
}
