{
  boolean match=true;
  boolean ones=true;
  for (int loc=pos - leftWindow(); loc <= pos + rightWindow(); loc++) {
    if (tags[loc] != correctTags[loc]) {
      match=false;
    }
    if (tags[loc] != 1 && loc >= leftWindow() && loc < length() + leftWindow()) {
      ones=false;
    }
  }
  if (match) {
    return pos;
  }
  if (ones) {
    return 0;
  }
  return 0;
}
