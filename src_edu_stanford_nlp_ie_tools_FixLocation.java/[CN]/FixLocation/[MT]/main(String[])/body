{
  if (args.length < 1) {
    System.err.println("Input filename?");
    inputFilename=in.readLine();
  }
 else {
    inputFilename=args[0];
  }
  if (args.length < 2) {
    System.err.println("Output filename?");
    outputFilename=in.readLine();
  }
 else {
    outputFilename=args[1];
  }
  String[][] cols=readFile(inputFilename);
  fix(cols);
  print(cols);
}
