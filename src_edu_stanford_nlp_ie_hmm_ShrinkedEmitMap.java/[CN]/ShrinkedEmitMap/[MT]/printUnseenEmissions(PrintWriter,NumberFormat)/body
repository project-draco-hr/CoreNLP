{
  base.printUnseenEmissions(out,nf);
  out.println();
  out.println("Shrink weights");
  out.println("--------------");
  out.println("lambda1 [base/me] --> " + nf.format(lambda1));
  out.println("lambda2 [parent]  --> " + nf.format(lambda2));
  out.println("lambda3 [uniform] --> " + nf.format(lambda3));
}
