{
  Optional<RelationTriple> extraction=mkExtraction("1\the\t3\tnsubjpass\n" + "2\twas\t3\tauxpass\n" + "3\tinaugurated\t0\troot\n"+ "5\tpresident\t3\tprep_as\n");
  assertTrue("No extraction for sentence!",extraction.isPresent());
  assertEquals("he\twas inaugurated as\tpresident",extraction.get().toString());
}
