{
  Optional<RelationTriple> extraction=mkExtraction("1\tfoal\t3\tnsubj\n" + "2\tbe\t3\taux\n" + "3\tstanding\t0\troot\n"+ "4\thorse\t3\tprep_next_to\n");
  assertTrue("No extraction for sentence!",extraction.isPresent());
  assertEquals("1.0\tfoal\tbe standing next to\thorse",extraction.get().toString());
}
