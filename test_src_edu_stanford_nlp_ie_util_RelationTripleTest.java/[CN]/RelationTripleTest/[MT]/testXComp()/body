{
  Optional<RelationTriple> extraction=mkExtraction("1\tObama\t3\tnsubjpass\n" + "2\twas\t3\tauxpass\n" + "3\tnamed\t0\troot\n"+ "4\t2009\t8\tnum\n"+ "5\tNobel\t8\tnn\n"+ "6\tPeace\t8\tnn\n"+ "7\tPrize\t8\tnn\n"+ "8\tLaureate\t3\txcomp\n");
  assertTrue("No extraction for sentence!",extraction.isPresent());
  assertEquals("1.0\tObama\twas named\t2009 Nobel Peace Prize Laureate",extraction.get().toString());
}
