{
  Optional<RelationTriple> extraction=mkExtraction("1\tUnited\t2\tnn\tNNP\tLOCATION\n" + "2\tStates\t4\tnsubj\tNNP\tLOCATION\n" + "3\t's\t2\tposs\tPOS\tO\n"+ "4\tpresident\t0\troot\tNN\tO\n"+ "5\tObama\t2\tappos\tNNP\tPERSON\n");
  assertTrue("No extraction for sentence!",extraction.isPresent());
  assertEquals("1.0\tObama\tis president of\tUnited States",extraction.get().toString());
}
