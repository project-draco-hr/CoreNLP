{
  Optional<RelationTriple> extraction=mkExtraction("1\tUnited\t4\tnn\tNNP\tLOCATION\n" + "2\tStates\t4\tnn\tNNP\tLOCATION\n" + "3\tally\t4\tnn\tNN\tO\n"+ "4\tBritain\t0\troot\tNNP\tLOCATION\n");
  assertTrue("No extraction for sentence!",extraction.isPresent());
  assertEquals("1.0\tBritain\tis ally of\tUnited States",extraction.get().toString());
}
