{
  Optional<RelationTriple> extraction=mkExtraction("1\tObama\t0\troot\tNNP\tPERSON\n" + "2\tTucson\t1\tprep_in\n");
  assertTrue("No extraction for sentence!",extraction.isPresent());
  assertEquals("1.0\tObama\tin\tTucson",extraction.get().toString());
  extraction=mkExtraction("1\tPietro\t2\tnn\tNNP\tPERSON\n" + "2\tBadoglio\t0\troot\tNNP\tPERSON\n" + "3\tsouthern\t4\tamod\n"+ "4\tItaly\t2\tprep_in\n");
  assertTrue("No extraction for sentence!",extraction.isPresent());
  assertEquals("1.0\tPietro Badoglio\tin\tsouthern Italy",extraction.get().toString());
}
