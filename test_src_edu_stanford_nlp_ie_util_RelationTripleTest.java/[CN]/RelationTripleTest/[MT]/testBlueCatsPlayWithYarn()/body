{
  Optional<RelationTriple> extraction=mkExtraction("1\tblue\t2\tamod\n" + "2\tcats\t3\tnsubj\n" + "3\tplay\t0\troot\n"+ "4\twith\t3\tprep\n"+ "5\tyarn\t4\tpobj\n");
  assertTrue("No extraction for sentence!",extraction.isPresent());
  assertEquals("1.0\tblue cats\tplay with\tyarn",extraction.get().toString());
}
