{
  Optional<RelationTriple> extraction=mkExtraction("1\tblue\t2\tamod\n" + "2\tcats\t3\tnsubj\n" + "3\tplay\t0\troot\n"+ "4\tquietly\t3\tadvmod\n"+ "6\tyarn\t3\tnmod:with\n");
  assertTrue("No extraction for sentence!",extraction.isPresent());
  assertEquals("1.0\tblue cats\tplay quietly with\tyarn",extraction.get().toString());
}
