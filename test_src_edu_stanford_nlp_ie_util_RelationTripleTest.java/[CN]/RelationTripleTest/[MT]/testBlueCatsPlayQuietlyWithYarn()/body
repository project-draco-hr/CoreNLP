{
  Optional<RelationTriple> extraction=mkExtraction("1\tblue\t2\tamod\n" + "2\tcats\t3\tnsubj\n" + "3\tplay\t0\troot\n"+ "4\tquietly\t3\tadvmod\n"+ "5\twith\t3\tprep\n"+ "6\tyarn\t5\tpobj\n");
  assertTrue("No extraction for sentence!",extraction.isPresent());
  assertEquals("blue cats\tplay quietly with\tyarn",extraction.get().toString());
}
