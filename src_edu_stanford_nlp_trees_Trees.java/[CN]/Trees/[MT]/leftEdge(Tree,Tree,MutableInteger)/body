{
  if (t == t1) {
    return true;
  }
 else   if (t1.isLeaf()) {
    int j=t1.yield().size();
    i.set(i.intValue() + j);
    return false;
  }
 else {
    Tree[] kids=t1.children();
    for (int j=0, n=kids.length; j < n; j++) {
      if (leftEdge(t,kids[j],i)) {
        return true;
      }
    }
    return false;
  }
}
