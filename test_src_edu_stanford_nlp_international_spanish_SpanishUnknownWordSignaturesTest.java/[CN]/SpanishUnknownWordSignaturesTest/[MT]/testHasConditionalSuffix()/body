{
  assertTrue(SpanishUnknownWordSignatures.hasConditionalSuffix("deber??a"));
  assertTrue(SpanishUnknownWordSignatures.hasConditionalSuffix("deber??as"));
  assertTrue(SpanishUnknownWordSignatures.hasConditionalSuffix("deber??amos"));
  assertTrue(SpanishUnknownWordSignatures.hasConditionalSuffix("deber??ais"));
  assertTrue(SpanishUnknownWordSignatures.hasConditionalSuffix("deber??an"));
  assertFalse(SpanishUnknownWordSignatures.hasConditionalSuffix("deb??a"));
  assertFalse(SpanishUnknownWordSignatures.hasConditionalSuffix("deb??as"));
  assertFalse(SpanishUnknownWordSignatures.hasConditionalSuffix("deb??amos"));
  assertFalse(SpanishUnknownWordSignatures.hasConditionalSuffix("deb??ais"));
  assertFalse(SpanishUnknownWordSignatures.hasConditionalSuffix("deb??an"));
}
