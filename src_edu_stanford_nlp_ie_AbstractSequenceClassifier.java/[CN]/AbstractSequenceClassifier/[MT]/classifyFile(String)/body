{
  ObjectBank<List<IN>> documents=makeObjectBankFromFile(filename,plainTextReaderAndWriter);
  List<List<IN>> result=new ArrayList<List<IN>>();
  for (  List<IN> document : documents) {
    classify(document);
    List<IN> sentence=new ArrayList<IN>();
    for (    IN wi : document) {
      sentence.add(wi);
    }
    result.add(sentence);
  }
  return result;
}
