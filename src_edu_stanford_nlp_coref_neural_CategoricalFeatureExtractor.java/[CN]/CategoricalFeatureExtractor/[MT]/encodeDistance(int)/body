{
  SimpleMatrix m=new SimpleMatrix(11,1);
  if (d < 5) {
    m.set(d,1);
  }
 else   if (d < 8) {
    m.set(5,1);
  }
 else   if (d < 16) {
    m.set(6,1);
  }
 else   if (d < 32) {
    m.set(7,1);
  }
 else   if (d < 64) {
    m.set(8,1);
  }
 else {
    m.set(9,1);
  }
  m.set(10,Math.min(d,64) / 64.0);
  return m;
}
