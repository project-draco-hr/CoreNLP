{
  int numParams=0;
  for (int i=0; i < transitions.size(); i++) {
    List<Double> curTransitions=transitions.get(i);
    for (int j=0; j < curTransitions.size(); j++) {
      Double curProb=curTransitions.get(j);
      if (curProb.doubleValue() != 0.0) {
        numParams++;
      }
    }
  }
  return numParams;
}
