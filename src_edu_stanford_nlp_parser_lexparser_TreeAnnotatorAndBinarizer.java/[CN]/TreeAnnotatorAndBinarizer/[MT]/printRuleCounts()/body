{
  System.err.println();
  for (  Tree t : annotatedRuleCounts.keySet()) {
    System.err.print(annotatedRuleCounts.getCount(t) + "\t" + t.label().value()+ " -->");
    for (    Tree dtr : t.getChildrenAsList()) {
      System.err.print(" ");
      System.err.print(dtr.label().value());
    }
    System.err.println();
  }
}
