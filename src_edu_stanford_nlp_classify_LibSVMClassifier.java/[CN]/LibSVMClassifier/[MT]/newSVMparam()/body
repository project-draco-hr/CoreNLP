{
  svm_parameter param=new svm_parameter();
  param.svm_type=svm_parameter.C_SVC;
  param.kernel_type=svm_parameter.RBF;
  param.degree=3;
  param.gamma=0;
  param.coef0=0;
  param.nu=0.5;
  param.cache_size=40;
  param.C=1;
  param.eps=1e-3;
  param.p=0.1;
  param.shrinking=1;
  param.nr_weight=0;
  param.weight_label=new int[0];
  param.weight=new double[0];
  param.probability=0;
  return param;
}
