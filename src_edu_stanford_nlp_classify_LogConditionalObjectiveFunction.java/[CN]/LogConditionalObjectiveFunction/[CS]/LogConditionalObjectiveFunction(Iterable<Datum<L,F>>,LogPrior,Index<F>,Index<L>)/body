{
  setPrior(logPrior);
  setUseSumCondObjFun(false);
  this.useIterable=true;
  this.numFeatures=featureIndex.size();
  this.numClasses=labelIndex.size();
  this.data=null;
  this.dataIterable=dataIterable;
  this.labelIndex=labelIndex;
  this.featureIndex=featureIndex;
  this.labels=null;
  this.values=null;
}
