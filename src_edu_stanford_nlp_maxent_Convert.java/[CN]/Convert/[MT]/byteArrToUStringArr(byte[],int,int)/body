{
  String[] s=new String[length];
  int stringLen;
  for (int i=0; i < length; i++) {
    stringLen=byteArrToInt(b,off);
    off+=4;
    if (stringLen != 0) {
      s[i]=byteArrToUString(b,off,stringLen);
      off+=2 * s[i].length();
    }
 else {
      s[i]=new String("");
    }
  }
  return s;
}
