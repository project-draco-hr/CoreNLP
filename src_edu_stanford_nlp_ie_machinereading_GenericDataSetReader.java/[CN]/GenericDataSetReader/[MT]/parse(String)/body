{
  Annotation retVal;
  try {
    retVal=this.read(path);
  }
 catch (  Exception ex) {
    IOException iox=new IOException();
    iox.initCause(ex);
    throw iox;
  }
  if (preProcessSentences) {
    preProcessSentences(retVal);
    if (MachineReadingProperties.trainUsePipelineNER)     modifyUsingCoreNLPNER(retVal);
  }
  return retVal;
}
