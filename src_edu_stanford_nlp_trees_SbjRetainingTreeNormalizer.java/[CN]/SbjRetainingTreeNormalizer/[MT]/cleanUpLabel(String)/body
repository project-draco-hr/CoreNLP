{
  if (label == null) {
    label="ROOT";
  }
 else {
    if (label.matches("^NP-SBJ")) {
      label=label.replaceAll("^NP-SBJ","NP#SBJ");
    }
    int k=label.indexOf('-');
    if (k > 0) {
      label=label.substring(0,k);
    }
    k=label.indexOf('=');
    if (k > 0) {
      label=label.substring(0,k);
    }
    k=label.indexOf('|');
    if (k > 0) {
      label=label.substring(0,k);
    }
  }
  return label;
}
