{
  return ("pos.maxlen:" + props.getProperty("pos.maxlen","") + "pos.verbose:"+ PropertiesUtils.getBool(props,"pos.verbose")+ "pos.reuseTags:"+ PropertiesUtils.getBool(props,"pos.reuseTags")+ "pos.model:"+ props.getProperty("pos.model",DefaultPaths.DEFAULT_POS_MODEL)+ "pos.nthreads:"+ props.getProperty("pos.nthreads",props.getProperty("nthreads","")));
}
