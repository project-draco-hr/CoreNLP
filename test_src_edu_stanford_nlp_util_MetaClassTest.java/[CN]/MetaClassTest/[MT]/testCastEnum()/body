{
  assertEquals(Fruits.APPLE,MetaClass.cast("APPLE",Fruits.class));
  assertEquals(Fruits.APPLE,MetaClass.cast("apple",Fruits.class));
  assertEquals(Fruits.APPLE,MetaClass.cast("Apple",Fruits.class));
  assertEquals(Fruits.APPLE,MetaClass.cast("aPPlE",Fruits.class));
  assertEquals(Fruits.Orange,MetaClass.cast("orange",Fruits.class));
  assertEquals(Fruits.grape,MetaClass.cast("grape",Fruits.class));
  assertEquals(Fruits.grape,MetaClass.cast("Grape",Fruits.class));
  assertEquals(Fruits.grape,MetaClass.cast("GRAPE",Fruits.class));
}
