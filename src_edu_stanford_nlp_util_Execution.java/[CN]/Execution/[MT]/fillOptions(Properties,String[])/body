{
  Properties options=StringUtils.argsToProperties(args);
  for (  String key : props.stringPropertyNames()) {
    options.put(key,props.getProperty(key));
  }
  fillOptionsImpl(null,BOOTSTRAP_CLASSES,options,false);
  Class<?>[] visibleClasses=optionClasses;
  if (visibleClasses == null)   visibleClasses=getVisibleClasses();
  fillOptionsImpl(null,visibleClasses,options);
}
