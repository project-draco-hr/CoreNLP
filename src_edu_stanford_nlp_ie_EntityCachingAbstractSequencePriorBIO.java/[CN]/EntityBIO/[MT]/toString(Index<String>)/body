{
  StringBuffer sb=new StringBuffer();
  sb.append("\"");
  sb.append(StringUtils.join(words," "));
  sb.append("\" start: ");
  sb.append(startPosition);
  sb.append(" type: ");
  sb.append(tagIndex.get(type));
  sb.append(" other_occurrences: ");
  sb.append(Arrays.toString(otherOccurrences));
  return sb.toString();
}
