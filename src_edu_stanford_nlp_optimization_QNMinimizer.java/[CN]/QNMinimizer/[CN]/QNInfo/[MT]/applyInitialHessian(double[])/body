{
switch (scaleOpt) {
case SCALAR:
    say("I");
  ArrayMath.multiplyInPlace(x,gamma);
break;
case DIAGONAL:
say("D");
if (d != null) {
if (x.length != d.length) {
throw new IllegalArgumentException("Vector of incorrect size passed to applyInitialHessian in QNInfo class");
}
for (int i=0; i < x.length; i++) {
x[i]=x[i] / (d[i]);
}
}
break;
}
return x;
}
