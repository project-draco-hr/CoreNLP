{
  double score;
  if (data[pos] == 7 && tags[pos] == 2 && tags[pos + 1] == 2) {
    score=1.0;
  }
 else   if (data[pos] == 8 && tags[pos] == 2) {
    score=0.5;
  }
 else   if (tags[pos] == 1) {
    score=0.1;
  }
 else   if (tags[pos] == 2) {
    score=-5.0;
  }
 else {
    score=0.0;
  }
  return score;
}
