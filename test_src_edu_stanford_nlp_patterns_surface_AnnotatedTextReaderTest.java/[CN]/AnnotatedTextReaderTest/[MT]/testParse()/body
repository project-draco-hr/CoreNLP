{
  try {
    String text="I am going to be in <LOC> Italy </LOC> sometime, soon. Specifically in <LOC> Tuscany </LOC> .";
    Set<String> labels=new HashSet<String>();
    labels.add("LOC");
    System.out.println(AnnotatedTextReader.parseFile(new BufferedReader(new StringReader(text)),labels,null,true,true,false,""));
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
