{
  StringBuffer sb=new StringBuffer("[");
  for (int i=0; i < targetFields.length; i++) {
    sb.append(targetFields[i]);
    if (i != targetFields.length - 1) {
      sb.append(", ");
    }
  }
  sb.append("]");
  return "Corpus{" + wordCount() + " tokens; "+ vocabSize()+ " types; "+ size()+ " documents; targets = "+ sb+ "}";
}
