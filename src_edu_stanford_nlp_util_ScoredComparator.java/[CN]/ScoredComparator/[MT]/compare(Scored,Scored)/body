{
  if (o1 == o2) {
    return 0;
  }
  double d1=o1.score();
  double d2=o2.score();
  if (ascending) {
    if (d1 < d2) {
      return -1;
    }
    if (d1 > d2) {
      return 1;
    }
  }
 else {
    if (d1 < d2) {
      return 1;
    }
    if (d1 > d2) {
      return -1;
    }
  }
  return 0;
}
