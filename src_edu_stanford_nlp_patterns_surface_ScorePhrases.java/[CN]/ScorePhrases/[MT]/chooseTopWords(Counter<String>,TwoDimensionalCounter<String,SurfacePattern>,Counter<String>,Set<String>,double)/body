{
  Iterator<String> termIter=Counters.toPriorityQueue(newdt).iterator();
  Counter<String> finalwords=new ClassicCounter<String>();
  while (termIter.hasNext()) {
    if (finalwords.size() >= numWordsToAdd)     break;
    String w=termIter.next();
    if (newdt.getCount(w) < thresholdWordExtract) {
      break;
    }
    assert(newdt.getCount(w) != Double.POSITIVE_INFINITY);
    if (useThresholdNumPatternsForTheseWords.containsKey(w) && numNonRedundantPatterns(terms,w) < thresholdNumPatternsApplied) {
      Redwood.log("extremePatDebug",channelNameLogger,"Not adding " + w + " because the number of non redundant patterns are below threshold: "+ terms.getCounter(w).keySet());
      continue;
    }
    String matchedFuzzy=null;
    if (minLen4FuzzyForPattern > 0 && ignoreWords != null)     matchedFuzzy=ConstantsAndVariables.containsFuzzy(ignoreWords,w,minLen4FuzzyForPattern);
    if (matchedFuzzy == null) {
      Redwood.log("extremePatDebug","adding word " + w);
      finalwords.setCount(w,newdt.getCount(w));
    }
 else {
      Redwood.log("extremePatDebug",channelNameLogger,"not adding " + w + " because it matched "+ matchedFuzzy+ " in common English word");
      ignoreWords.add(w);
    }
  }
  return finalwords;
}
