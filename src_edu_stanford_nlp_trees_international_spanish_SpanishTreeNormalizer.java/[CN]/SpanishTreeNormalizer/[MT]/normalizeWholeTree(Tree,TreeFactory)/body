{
  for (  Tree t : tree) {
    if (t.isPreTerminal()) {
      CoreLabel label=(CoreLabel)t.label();
      String pos=label.value();
      pos=simplifyPOSTag(pos).intern();
      label.setValue(pos);
      label.setTag(pos);
    }
 else     if (t.isPrePreTerminal()) {
      normalizeForMultiWord(t,tf);
    }
  }
  return tree;
}
