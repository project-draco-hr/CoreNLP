{
  List<Pair<TregexPattern,TsurgeonPattern>> ret=new ArrayList<Pair<TregexPattern,TsurgeonPattern>>(patterns.length);
  for (  Pair<String,String> pattern : patterns)   ret.add(new Pair<TregexPattern,TsurgeonPattern>(TregexPattern.compile(pattern.first()),Tsurgeon.parseOperation(pattern.second())));
  return ret;
}
