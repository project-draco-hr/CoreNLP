{
  this.props=props;
  if (generalizeClasses.isEmpty()) {
    for (    String label : seedSets.keySet())     generalizeClasses.put(label,new HashMap<String,Class>());
  }
  if (ignoreClasses.isEmpty()) {
    for (    String label : seedSets.keySet())     ignoreClasses.put(label,new HashMap<Class,Object>());
  }
  setUpConstructor(sents,seedSets,answerClass,generalizeClasses,ignoreClasses);
}
