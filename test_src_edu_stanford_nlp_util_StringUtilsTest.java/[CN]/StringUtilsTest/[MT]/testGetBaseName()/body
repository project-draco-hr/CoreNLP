{
  assertEquals(StringUtils.getBaseName("/u/wcmac/foo.txt"),"foo.txt");
  assertEquals(StringUtils.getBaseName("/u/wcmac/foo.txt",""),"foo.txt");
  assertEquals(StringUtils.getBaseName("/u/wcmac/foo.txt",".txt"),"foo");
  assertEquals(StringUtils.getBaseName("/u/wcmac/foo.txt",".pdf"),"foo.txt");
}
