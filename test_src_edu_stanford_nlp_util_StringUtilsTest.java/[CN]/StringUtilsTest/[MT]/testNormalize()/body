{
  assertEquals("can't",StringUtils.normalize("can't"));
  assertEquals("Beyonce",StringUtils.normalize("Beyonc??"));
  assertEquals("krouzek",StringUtils.normalize("krou??ek"));
  assertEquals("office",StringUtils.normalize("o\uFB03ce"));
  assertEquals("DZ",StringUtils.normalize("??"));
  assertEquals("1???4",StringUtils.normalize("??"));
  assertEquals("?????????",StringUtils.normalize("?????????"));
  assertEquals("?????????",StringUtils.normalize("?????????"));
  assertEquals("???",StringUtils.normalize("???"));
  assertEquals("???",StringUtils.normalize("???"));
}
