{
  if (sentence.containsKey(SemanticGraphCoreAnnotations.BasicDependenciesAnnotation.class) || sentence.containsKey(SemanticGraphCoreAnnotations.CollapsedDependenciesAnnotation.class)) {
    annotateOperators(sentence);
    annotateUnaries(sentence);
    if (doPolarity) {
      annotatePolarity(sentence);
    }
  }
}
