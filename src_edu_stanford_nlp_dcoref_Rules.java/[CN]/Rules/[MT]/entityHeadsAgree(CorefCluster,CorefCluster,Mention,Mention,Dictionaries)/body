{
  boolean headAgree=false;
  if (m.isPronominal() || ant.isPronominal() || dict.allPronouns.contains(m.spanToString().toLowerCase())|| dict.allPronouns.contains(ant.spanToString().toLowerCase()))   return false;
  for (  Mention a : potentialAntecedent.corefMentions) {
    if (a.headString.equals(m.headString))     headAgree=true;
  }
  return headAgree;
}
