{
  Map<OUT,String> pads=new HashMap<OUT,String>();
  int max=0;
  for (  OUT key : keys) {
    String keyString=key == null ? "null" : key.toString();
    if (keyString.length() > max) {
      max=keyString.length();
    }
  }
  for (  OUT key : keys) {
    String keyString=key == null ? "null" : key.toString();
    int diff=max - keyString.length();
    String pad="";
    for (int j=0; j < diff; j++) {
      pad+=" ";
    }
    pads.put(key,pad);
  }
  return pads;
}
