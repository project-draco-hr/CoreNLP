{
switch (format) {
case "xml":
    return "  <dep>\n    <governor>" + XMLUtils.escapeXML(governor().value()) + "</governor>\n    <dependent>"+ XMLUtils.escapeXML(dependent().value())+ "</dependent>\n  </dep>";
case "predicate":
  return "dep(" + governor() + ","+ dependent()+ ")";
default :
return toString();
}
}
