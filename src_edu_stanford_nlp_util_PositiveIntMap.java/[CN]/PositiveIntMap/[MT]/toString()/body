{
  StringBuilder b=new StringBuilder();
  for (int i=0; i < map.length; i++) {
    if (map[i] < 0)     continue;
    if (b.length() == 0) {
      b.append("{");
    }
 else {
      b.append(",");
    }
    int value=map[i];
    b.append((Object)i).append("=").append(value);
  }
  b.append("}");
  return b.toString();
}
