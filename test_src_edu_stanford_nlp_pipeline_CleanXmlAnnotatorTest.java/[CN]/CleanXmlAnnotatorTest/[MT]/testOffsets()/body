{
  String testString="<p><p>This text is in a</p>nested tag</p>";
  Annotation annotation=annotate(testString,ptbInvertible,cleanXmlAllTags,wtsSplitter);
  checkResult(annotation,"This text is in a nested tag");
  List<CoreLabel> labels=annotation.get(TokensAnnotation.class);
  assertEquals(6,labels.get(0).get(CharacterOffsetBeginAnnotation.class).intValue());
  assertEquals(10,labels.get(0).get(CharacterOffsetEndAnnotation.class).intValue());
}
