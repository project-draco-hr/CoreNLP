{
  String name=t.tagName;
  if (ignores.get(name) == inScope) {
    ignores.put(name,notInScope);
    return;
  }
  if (!scriptInScope() && (acceptableTags == null || acceptableTags.contains(name))) {
    newLinePrinted=false;
    if (ignoreAttributes && !name.equals("tag")) {
      out.print("</" + name + ">");
    }
 else {
      out.print(t);
    }
  }
}
