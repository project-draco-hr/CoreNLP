{
  if (!scriptInScope()) {
    newLinePrinted=false;
    t.text=t.text.replaceAll("&nbsp;"," ");
    t.text=t.text.replaceAll("&lt;","<");
    t.text=t.text.replaceAll("&gt;",">");
    t.text=t.text.replaceAll("&amp;","&");
    t.text=t.text.replaceAll("&quot","\"");
    t.text=t.text.replaceAll("&apos","'");
    out.print(t);
  }
}
