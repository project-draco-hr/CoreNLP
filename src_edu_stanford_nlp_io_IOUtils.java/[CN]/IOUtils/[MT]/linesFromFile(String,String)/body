{
  try {
    List<String> lines=new ArrayList<String>();
    BufferedReader in=new BufferedReader(new EncodingFileReader(filename,encoding));
    String line;
    while ((line=in.readLine()) != null) {
      lines.add(line);
    }
    in.close();
    return lines;
  }
 catch (  IOException e) {
    e.printStackTrace();
    return null;
  }
}
