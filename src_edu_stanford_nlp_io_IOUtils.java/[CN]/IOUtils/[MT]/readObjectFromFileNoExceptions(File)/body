{
  Object o=null;
  try {
    ObjectInputStream ois=new ObjectInputStream(new BufferedInputStream(new GZIPInputStream(new FileInputStream(file))));
    o=ois.readObject();
    ois.close();
  }
 catch (  IOException|ClassNotFoundException e) {
    e.printStackTrace();
  }
  return ErasureUtils.uncheckedCast(o);
}
