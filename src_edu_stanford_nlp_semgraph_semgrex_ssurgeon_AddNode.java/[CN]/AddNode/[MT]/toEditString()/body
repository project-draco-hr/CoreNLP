{
  StringWriter buf=new StringWriter();
  buf.write(LABEL);
  buf.write("\t");
  buf.write(Ssurgeon.NODE_PROTO_ARG);
  buf.write(" ");
  buf.write("\"");
  buf.write(nodeString);
  buf.write("\"\t");
  buf.write(Ssurgeon.NAME_ARG);
  buf.write("\t");
  buf.write(nodeName);
  return buf.toString();
}
