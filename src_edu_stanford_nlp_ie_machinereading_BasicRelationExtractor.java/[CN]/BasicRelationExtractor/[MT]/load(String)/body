{
  InputStream is=BasicRelationExtractor.class.getClassLoader().getResourceAsStream(modelPath);
  if (is == null)   is=new FileInputStream(modelPath);
  ObjectInputStream in=new ObjectInputStream(is);
  BasicRelationExtractor bre=ErasureUtils.<BasicRelationExtractor>uncheckedCast(in.readObject());
  in.close();
  is.close();
  return bre;
}
