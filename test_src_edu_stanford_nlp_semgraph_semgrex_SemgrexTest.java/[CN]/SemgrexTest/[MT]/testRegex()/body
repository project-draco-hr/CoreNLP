{
  runTest("{word:/Bill/}","[ate subj:Bill dobj:[muffins nn:blueberry]]","Bill");
  runTest("{word:/ill/}","[ate subj:Bill dobj:[muffins nn:blueberry]]");
  runTest("{word:/.*ill/}","[ate subj:Bill dobj:[muffins nn:blueberry]]","Bill");
  runTest("{word:/.*il/}","[ate subj:Bill dobj:[muffins nn:blueberry]]");
  runTest("{word:/.*il.*/}","[ate subj:Bill dobj:[muffins nn:blueberry]]","Bill");
}
