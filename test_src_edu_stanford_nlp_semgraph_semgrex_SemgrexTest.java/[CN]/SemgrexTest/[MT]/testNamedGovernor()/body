{
  runTest("{word:blueberry} << {}","[ate subj:Bill dobj:[muffins nn:blueberry]]","blueberry");
  runTest("{word:ate} << {}","[ate subj:Bill dobj:[muffins nn:blueberry]]");
  runTest("{word:blueberry} >> {}","[ate subj:Bill dobj:[muffins nn:blueberry]]");
  runTest("{word:muffins} >> {}","[ate subj:Bill dobj:[muffins nn:blueberry]]","muffins");
  runTest("{word:Bill} >> {}","[ate subj:Bill dobj:[muffins nn:blueberry]]");
  runTest("{word:muffins} < {}","[ate subj:Bill dobj:[muffins nn:blueberry]]","muffins");
  runTest("{word:muffins} > {}","[ate subj:Bill dobj:[muffins nn:blueberry]]","muffins");
}
