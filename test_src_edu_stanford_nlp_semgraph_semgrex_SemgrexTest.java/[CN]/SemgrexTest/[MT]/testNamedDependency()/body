{
  runTest("{} << {word:ate}","[ate subj:Bill dobj:[muffins nn:blueberry]]","Bill","muffins","blueberry");
  runTest("{} >> {word:blueberry}","[ate subj:Bill dobj:[muffins nn:blueberry]]","ate","muffins");
  runTest("{} >> {word:Bill}","[ate subj:Bill dobj:[muffins nn:blueberry]]","ate");
  runTest("{} < {word:ate}","[ate subj:Bill dobj:[muffins nn:blueberry]]","Bill","muffins");
  runTest("{} > {word:blueberry}","[ate subj:Bill dobj:[muffins nn:blueberry]]","muffins");
  runTest("{} > {word:muffins}","[ate subj:Bill dobj:[muffins nn:blueberry]]","ate");
}
