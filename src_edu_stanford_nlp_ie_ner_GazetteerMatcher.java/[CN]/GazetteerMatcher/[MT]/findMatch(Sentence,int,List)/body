{
  for (int i=0; i < gazettes.size(); i++) {
    String[] g=((String)((Pair)gazettes.get(i)).second()).split(" ");
    for (int j=0; j <= g.length; j++) {
      if (j == g.length) {
        mark(s,pos,j,(String)((Pair)gazettes.get(i)).first());
        continue;
      }
      if ((pos + j) >= s.size()) {
        break;
      }
      if (!g[j].equalsIgnoreCase(((Word)s.get(pos + j)).word)) {
        break;
      }
    }
  }
}
