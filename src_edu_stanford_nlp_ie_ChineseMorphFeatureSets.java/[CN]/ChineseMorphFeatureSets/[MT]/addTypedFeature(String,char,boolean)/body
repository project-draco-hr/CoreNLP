{
  Pair<Set<Character>,Set<Character>> p=affixFeatures.get(featName);
  if (p == null) {
    affixFeatures.put(featName,p=new Pair<Set<Character>,Set<Character>>());
  }
  Set<Character> feature;
  if (isPrefix) {
    feature=p.first();
    if (feature == null) {
      p.setFirst(feature=new HashSet<Character>());
    }
  }
 else {
    feature=p.second();
    if (feature == null) {
      p.setSecond(feature=new HashSet<Character>());
    }
  }
  feature.add(featChar);
}
