{
  SemanticGraph sg=new SemanticGraph(list);
  if (DEBUG) {
    printListSorted("At postProcessDependencies:",sg.typedDependencies());
  }
  convertRel(sg);
  if (DEBUG) {
    printListSorted("After converting rel:",sg.typedDependencies());
  }
  list.clear();
  list.addAll(sg.typedDependencies());
}
