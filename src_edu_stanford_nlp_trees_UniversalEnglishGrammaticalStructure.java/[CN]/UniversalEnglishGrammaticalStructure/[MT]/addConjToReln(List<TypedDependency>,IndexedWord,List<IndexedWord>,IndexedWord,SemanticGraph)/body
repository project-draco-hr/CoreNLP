{
  for (  IndexedWord conjDep : conjDeps) {
    for (    TypedDependency td : list) {
      if (!td.gov().equals(gov) || !td.dep().equals(conjDep)) {
        continue;
      }
      if (td.reln() == CONJUNCT || conjDep.index() > ccDep.index()) {
        td.setReln(conjValue(ccDep,sg));
      }
      break;
    }
  }
}
