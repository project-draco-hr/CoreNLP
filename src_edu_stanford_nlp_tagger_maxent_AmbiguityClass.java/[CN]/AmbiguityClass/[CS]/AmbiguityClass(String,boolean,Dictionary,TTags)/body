{
  this.single=single;
  if (single) {
    this.word=word;
    sortedIds=Collections.emptyList();
  }
 else {
    this.word=null;
    String[] tags=dict.getTags(word);
    sortedIds=new ArrayList<Integer>(tags.length);
    for (    String tag : tags) {
      add(ttags.getIndex(tag));
    }
  }
  key=this.toString();
}
