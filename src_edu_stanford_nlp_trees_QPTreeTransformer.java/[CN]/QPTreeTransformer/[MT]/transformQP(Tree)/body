{
  List<Tree> children=t.getChildrenAsList();
  TreeFactory tf=t.treeFactory();
  LabelFactory lf=t.label().labelFactory();
  Tree left=tf.newTreeNode(lf.newLabel("XS"),null);
  for (int i=0; i < 2; i++) {
    left.addChild(children.get(i));
  }
  for (int i=0; i < 2; i++) {
    t.removeChild(0);
  }
  t.addChild(0,left);
}
