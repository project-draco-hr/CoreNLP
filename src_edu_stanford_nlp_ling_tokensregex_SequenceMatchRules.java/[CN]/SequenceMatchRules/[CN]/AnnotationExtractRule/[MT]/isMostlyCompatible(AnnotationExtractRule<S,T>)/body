{
  return (stage == aer.stage && Objects.equals(annotationField,aer.annotationField) && Objects.equals(tokensAnnotationField,aer.tokensAnnotationField) && matchedExpressionGroup == 0 && aer.matchedExpressionGroup == 0 && matchWithResults == aer.matchWithResults && Objects.equals(ruleType,aer.ruleType) && isComposite == aer.isComposite && active == aer.active && Objects.equals(result,aer.result));
}
