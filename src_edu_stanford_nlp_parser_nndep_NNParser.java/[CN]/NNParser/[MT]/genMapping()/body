{
  wordMap=new HashMap<String,Integer>();
  posMap=new HashMap<String,Integer>();
  labelMap=new HashMap<String,Integer>();
  int index=0;
  for (int i=0; i < wordDict.size(); ++i)   wordMap.put(wordDict.get(i),(index++));
  for (int i=0; i < posDict.size(); ++i)   posMap.put(posDict.get(i),(index++));
  for (int i=0; i < labelDict.size(); ++i)   labelMap.put(labelDict.get(i),(index++));
}
