{
  wordIDs=new HashMap<String,Integer>();
  posIDs=new HashMap<String,Integer>();
  labelIDs=new HashMap<String,Integer>();
  int index=0;
  for (int i=0; i < knownWords.size(); ++i)   wordIDs.put(knownWords.get(i),(index++));
  for (int i=0; i < knownPos.size(); ++i)   posIDs.put(knownPos.get(i),(index++));
  for (int i=0; i < knownLabels.size(); ++i)   labelIDs.put(knownLabels.get(i),(index++));
}
