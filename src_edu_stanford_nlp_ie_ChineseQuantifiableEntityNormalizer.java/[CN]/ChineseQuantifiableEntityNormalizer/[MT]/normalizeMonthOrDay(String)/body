{
  if (s == null) {
    return "XX";
  }
 else {
    String candidate;
    if (CHINESE_LITERAL_DECIMAL_PATTERN.matcher(s).find())     candidate=prettyNumber(String.format("%f",normalizeLiteralDecimalString(s)));
 else     candidate=s;
    if (candidate.length() < 2)     candidate="0" + candidate;
    return candidate;
  }
}
