{
  InputStream ins=null;
  try {
    ins=new BufferedInputStream(new FileInputStream(in));
    transformXML(tags,fn,ins,System.out);
  }
 catch (  Exception e) {
    System.err.println("Error reading file " + in + ": "+ e);
    e.printStackTrace();
  }
 finally {
    IOUtils.closeIgnoringExceptions(ins);
  }
}
