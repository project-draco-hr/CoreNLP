{
  Set<String> sentids=new HashSet<String>();
  for (  String w : words) {
    if (matchLowerCaseContext)     w=w.toLowerCase();
    Set<String> st=index.get(w);
    if (st == null)     throw new RuntimeException("How come the index does not have sentences for " + w);
    sentids.addAll(st);
  }
  return sentids;
}
