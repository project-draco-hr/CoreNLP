{
  if (other == edu.stanford.nlp.pipeline.CoreNLPProtos.SpeakerInfo.getDefaultInstance())   return this;
  if (other.hasSpeakerName()) {
    bitField0_|=0x00000001;
    speakerName_=other.speakerName_;
    onChanged();
  }
