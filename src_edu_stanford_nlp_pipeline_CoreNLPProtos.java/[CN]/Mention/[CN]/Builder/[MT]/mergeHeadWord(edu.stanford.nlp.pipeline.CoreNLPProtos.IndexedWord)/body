{
  if (headWordBuilder_ == null) {
    if (((bitField0_ & 0x40000000) == 0x40000000) && headWord_ != null && headWord_ != edu.stanford.nlp.pipeline.CoreNLPProtos.IndexedWord.getDefaultInstance()) {
      headWord_=edu.stanford.nlp.pipeline.CoreNLPProtos.IndexedWord.newBuilder(headWord_).mergeFrom(value).buildPartial();
    }
 else {
      headWord_=value;
    }
    onChanged();
  }
 else {
    headWordBuilder_.mergeFrom(value);
  }
  bitField0_|=0x40000000;
  return this;
}
