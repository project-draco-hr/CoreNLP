{
  if (dependingVerbBuilder_ == null) {
    if (((bitField0_ & 0x20000000) == 0x20000000) && dependingVerb_ != edu.stanford.nlp.pipeline.CoreNLPProtos.IndexedWord.getDefaultInstance()) {
      dependingVerb_=edu.stanford.nlp.pipeline.CoreNLPProtos.IndexedWord.newBuilder(dependingVerb_).mergeFrom(value).buildPartial();
    }
 else {
      dependingVerb_=value;
    }
    onChanged();
  }
 else {
    dependingVerbBuilder_.mergeFrom(value);
  }
  bitField0_|=0x20000000;
  return this;
}
