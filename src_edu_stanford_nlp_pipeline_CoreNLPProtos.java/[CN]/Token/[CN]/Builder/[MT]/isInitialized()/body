{
  if (!hasWord()) {
    return false;
  }
  if (hasOperator()) {
    if (!getOperator().isInitialized()) {
      return false;
    }
  }
  if (hasPolarity()) {
    if (!getPolarity().isInitialized()) {
      return false;
    }
  }
  if (!extensionsAreInitialized()) {
    return false;
  }
  return true;
}
