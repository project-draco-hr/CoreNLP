{
  if (polarityBuilder_ == null) {
    if (((bitField0_ & 0x04000000) == 0x04000000) && polarity_ != edu.stanford.nlp.pipeline.CoreNLPProtos.Polarity.getDefaultInstance()) {
      polarity_=edu.stanford.nlp.pipeline.CoreNLPProtos.Polarity.newBuilder(polarity_).mergeFrom(value).buildPartial();
    }
 else {
      polarity_=value;
    }
    onChanged();
  }
 else {
    polarityBuilder_.mergeFrom(value);
  }
  bitField0_|=0x04000000;
  return this;
}
