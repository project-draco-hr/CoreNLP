{
  this(properties,new ArrayList<Backend>(){
{
      for (int i=0; i < threads; ++i) {
        add(new Backend(host.startsWith("https://") ? "https" : "http",host.startsWith("http://") ? host.substring("http://".length()) : (host.startsWith("https://") ? host.substring("https://".length()) : host),port));
      }
    }
  }
);
}
