{
  if (k < 0 || k > tree.n)   return CONST.NULL;
  HashSet<String> labelSet=new HashSet<String>();
  for (int i=1; i < k; ++i)   if (tree.getHead(i) == k)   labelSet.add(tree.getLabel(i));
  List<String> ls=new ArrayList<String>(labelSet);
  Collections.sort(ls);
  String s="";
  for (int i=0; i < ls.size(); ++i)   s=s + "/" + ls.get(i);
  return s;
}
