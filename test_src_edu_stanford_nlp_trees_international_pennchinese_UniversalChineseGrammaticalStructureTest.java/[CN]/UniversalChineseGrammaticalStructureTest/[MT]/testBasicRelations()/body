{
  Pair<String,String>[] examples=new Pair[]{T("(NP (DNP (NP (NP (NR ??????)) (NP (NN ??????))) (DEG ???)) (ADJP (JJ ??????)) (NP (NN ??????)))",C("nn(??????-2, ??????-1)","assmod(??????-5, ??????-2)","case(??????-2, ???-3)","amod(??????-5, ??????-4)","root(ROOT-0, ??????-5)")),T("(ROOT (IP (NP (NP (NR ??????) (NR ??????)) (NP (NN ??????) (CC ???) (NN ??????) (NN ??????))) (VP (VV ??????))))",C("nn(??????-2, ??????-1)","nn(??????-6, ??????-2)","conj(??????-6, ??????-3)","cc(??????-6, ???-4)","nn(??????-6, ??????-5)","nsubj(??????-7, ??????-6)","root(ROOT-0, ??????-7)")),T("(LCP (NP (NT ??????)) (LC ???))",C("root(ROOT-0, ??????-1)","case(??????-1, ???-2)")),T("(PP (P ??????) (NP (DNP (NP (NP (NN ??????)) (CC ???) (NP (NR ?????????))) (DEG ???)) (ADJP (JJ ??????)) (NP (NN ??????))))",C("case(??????-7, ??????-1)","conj(?????????-4, ??????-2)","cc(?????????-4, ???-3)","assmod(??????-7, ?????????-4)","case(?????????-4, ???-5)","amod(??????-7, ??????-6)","root(ROOT-0, ??????-7)")),T("(IP (NP (NN ??????)) (VP (VC ???) (NP (CP (IP (VP (VV ??????) (NP (NR ??????)))) (DEC ???)) (QP (CD ???) (CLP (M ???))) (ADJP (JJ ??????)) (NP (NN ??????) (NN ??????)))))",C("nsubj(??????-10, ??????-1)","cop(??????-10, ???-2)","relcl(??????-10, ??????-3)","dobj(??????-3, ??????-4)","mark(??????-3, ???-5)","nummod(???-7, ???-6)","clf(??????-10, ???-7)","amod(??????-10, ??????-8)","nn(??????-10, ??????-9)","root(ROOT-0, ??????-10)")),T("(IP (NP (NN ???)) (VP (SB ???) (VP (VV ??????) (NP (PU ???) (DNP (NP (ADJP (JJ ??????)) (NP (NN ??????))) (DEG ???)) (NP (NN ?????????)) (PU ???)))))",C("nsubjpass(??????-3, ???-1)","auxpass(??????-3, ???-2)","root(ROOT-0, ??????-3)","amod(??????-6, ??????-5)","assmod(?????????-8, ??????-6)","case(??????-6, ???-7)","dobj(??????-3, ?????????-8)")),T("(IP (VP (VP (ADVP (AD ???)) (VP (VV ??????))) (VP (ADVP (AD ???)) (VP (SB ???) (VP (VV ??????) (NP (NN ??????) (NN ??????)))))))))))",C("advmod(??????-2, ???-1)","root(ROOT-0, ??????-2)","advmod(??????-5, ???-3)","au