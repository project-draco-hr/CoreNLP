{
  int i=index - 1;
  while (i > 0 && Character.isWhitespace(sb.charAt(i))) {
    i--;
  }
  if (i < 0) {
    return false;
  }
  if (Character.isUpperCase(sb.charAt(i))) {
    return false;
  }
  int j=index + 1;
  while (j < sb.length() - 1 && Character.isWhitespace(sb.charAt(j))) {
    j++;
  }
  if (Character.isUpperCase(sb.charAt(j))) {
    return true;
  }
  return false;
}
