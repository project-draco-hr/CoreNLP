{
  String languageNameLower=languageName.toLowerCase();
  if (languageNameLower.equals("english") || languageNameLower.equals("en"))   return ENGLISH_PROPERTIES;
  if (languageNameLower.equals("chinese") || languageNameLower.equals("zh"))   return CHINESE_PROPERTIES;
 else   if (languageNameLower.equals("french") || languageNameLower.equals("fr"))   return FRENCH_PROPERTIES;
 else   if (languageNameLower.equals("german") || languageNameLower.equals("de"))   return GERMAN_PROPERTIES;
 else   if (languageNameLower.equals("spanish") || languageNameLower.equals("es"))   return SPANISH_PROPERTIES;
 else   return null;
}
