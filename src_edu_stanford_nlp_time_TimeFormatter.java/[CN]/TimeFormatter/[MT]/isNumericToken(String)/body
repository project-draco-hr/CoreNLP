{
  int tokenLen=token.length();
  if (tokenLen > 0) {
    char c=token.charAt(0);
switch (c) {
case 'c':
case 'C':
case 'x':
case 'y':
case 'Y':
case 'd':
case 'h':
case 'H':
case 'm':
case 's':
case 'S':
case 'e':
case 'D':
case 'F':
case 'w':
case 'W':
case 'k':
case 'K':
      return true;
case 'M':
    if (tokenLen <= 2) {
      return true;
    }
}
}
return false;
}
