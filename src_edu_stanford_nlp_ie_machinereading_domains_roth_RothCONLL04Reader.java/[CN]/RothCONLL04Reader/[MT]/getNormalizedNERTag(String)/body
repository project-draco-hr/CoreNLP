{
  if (ner.equalsIgnoreCase("O"))   return "O";
 else   if (ner.equalsIgnoreCase("Peop"))   return "PERSON";
 else   if (ner.equalsIgnoreCase("Loc"))   return "LOCATION";
 else   if (ner.equalsIgnoreCase("Org"))   return "ORGANIZATION";
 else   if (ner.equalsIgnoreCase("Other"))   return "OTHER";
  throw new RuntimeException("Cannot normalize ner tag " + ner);
}
