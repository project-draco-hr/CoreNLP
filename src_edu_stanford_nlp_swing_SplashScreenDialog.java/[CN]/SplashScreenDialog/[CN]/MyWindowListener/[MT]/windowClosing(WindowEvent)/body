{
  int option=JOptionPane.showConfirmDialog(frame,SC_CONFIRM_EXIT_TEXT,SC_CONFIRM_EXIT,JOptionPane.YES_NO_CANCEL_OPTION,JOptionPane.QUESTION_MESSAGE);
  if (option == JOptionPane.YES_OPTION) {
    System.exit(0);
  }
 else   if (option == JOptionPane.CANCEL_OPTION) {
    we.getWindow().setVisible(false);
  }
}
