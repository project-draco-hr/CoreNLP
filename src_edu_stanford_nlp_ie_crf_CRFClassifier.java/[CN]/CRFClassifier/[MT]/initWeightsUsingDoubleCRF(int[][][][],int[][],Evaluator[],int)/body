{
  CRFLogConditionalObjectiveFunction func=null;
  if ("DROPOUT".equalsIgnoreCase(flags.priorType)) {
    func=new CRFLogConditionalObjectiveFunctionWithDropout(data,labels,windowSize,classIndex,labelIndices,map,flags.priorType,flags.backgroundSymbol,flags.sigma,null,flags.dropoutRate,flags.dropoutScale,flags.multiThreadGrad,flags.dropoutApprox,flags.unsupDropoutScale,null);
  }
 else {
    func=new CRFLogConditionalObjectiveFunction(data,labels,windowSize,classIndex,labelIndices,map,flags.priorType,flags.backgroundSymbol,flags.sigma,null,flags.multiThreadGrad);
  }
  return func.initial();
}
