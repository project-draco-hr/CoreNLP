{
  CRFLogConditionalObjectiveFunction func=new CRFLogConditionalObjectiveFunction(data,labels,windowSize,classIndex,labelIndices,map,flags.priorType,flags.backgroundSymbol,flags.sigma,null,flags.dropoutRate,flags.dropoutScale,flags.multiThreadGrad,flags.dropoutApprox,flags.unsupDropoutScale,null);
  return func.initial();
}
