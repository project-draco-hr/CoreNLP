{
  final List<CoreLabel> words=sentence.get(CoreAnnotations.TokensAnnotation.class);
  Tree tree=ParserUtils.xTree(words);
  for (  CoreLabel word : words) {
    if (word.tag() == null) {
      word.setTag("XX");
    }
  }
  ScoredObject<Tree> so=new ScoredObject<>(tree,tree.score());
  List<ScoredObject<Tree>> trees=Generics.newArrayList(1);
  trees.add(so);
  finishSentence(sentence,trees);
}
