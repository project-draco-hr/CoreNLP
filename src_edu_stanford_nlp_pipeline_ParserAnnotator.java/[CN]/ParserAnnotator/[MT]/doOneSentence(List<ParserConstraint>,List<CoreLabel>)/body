{
  ParserQuery pq=parser.parserQuery();
  pq.setConstraints(constraints);
  pq.parse(words);
  List<ScoredObject<Tree>> trees=null;
  try {
    trees=pq.getKBestPCFGParses(this.kBest);
    if (trees == null || trees.size() < 1) {
      System.err.println("WARNING: Parsing of sentence failed.  " + "Will ignore and continue: " + Sentence.listToString(words));
    }
 else {
      for (      ScoredObject<Tree> so : trees) {
        so.setScore(so.score() % -10000.0);
        so.object().setScore(so.score());
      }
    }
  }
 catch (  OutOfMemoryError e) {
    System.err.println("WARNING: Parsing of sentence ran out of memory.  " + "Will ignore and continue: " + Sentence.listToString(words));
  }
catch (  NoSuchParseException e) {
    System.err.println("WARNING: Parsing of sentence failed, possibly because of out of memory.  " + "Will ignore and continue: " + Sentence.listToString(words));
  }
  return trees;
}
