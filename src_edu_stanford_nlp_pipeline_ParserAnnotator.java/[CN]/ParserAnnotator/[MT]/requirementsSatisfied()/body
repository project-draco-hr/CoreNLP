{
  if (this.BUILD_GRAPHS) {
    if (this.saveBinaryTrees) {
      return PARSE_TAG_DEPPARSE_BINARIZED_TREES;
    }
 else {
      return PARSE_TAG_DEPPARSE;
    }
  }
 else {
    if (this.saveBinaryTrees) {
      return PARSE_TAG_BINARIZED_TREES;
    }
 else {
      return PARSE_AND_TAG;
    }
  }
}
