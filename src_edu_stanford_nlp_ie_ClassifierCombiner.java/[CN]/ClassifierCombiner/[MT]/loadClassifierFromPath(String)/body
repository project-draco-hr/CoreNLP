{
  try {
    return ErasureUtils.uncheckedCast(CRFClassifier.getClassifier(path));
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  try {
    return ErasureUtils.uncheckedCast(CMMClassifier.getClassifier(path));
  }
 catch (  Exception e) {
    FileNotFoundException fnfe=new FileNotFoundException();
    fnfe.initCause(e);
    throw fnfe;
  }
}
