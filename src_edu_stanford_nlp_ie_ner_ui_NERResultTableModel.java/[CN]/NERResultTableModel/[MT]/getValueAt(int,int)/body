{
  NERResult result=(NERResult)filtered.get(row);
  Object value;
switch (col) {
case ID_COLUMN:
    value=result.getID();
  break;
case TEXT_COLUMN:
value=result.getText();
break;
case TP_COLUMN:
value=Integer.valueOf(result.getTP());
break;
case FP_COLUMN:
value=Integer.valueOf(result.getFP());
break;
case FN_COLUMN:
value=Integer.valueOf(result.getFN());
break;
default :
value=null;
}
return (value);
}
