{
  if (filter == null) {
    filtered=results;
  }
 else {
    filtered=new ArrayList();
    for (Iterator iter=results.iterator(); iter.hasNext(); ) {
      NERResult result=(NERResult)iter.next();
      if (!filter.filter(result)) {
        filtered.add(result);
      }
    }
  }
  fireTableDataChanged();
}
