{
  if (tree.isLeaf()) {
    return;
  }
  for (  Tree child : tree.children()) {
    countTree(child);
  }
  Integer gold=RNNCoreAnnotations.getGoldClass(tree);
  Integer predicted=RNNCoreAnnotations.getPredictedClass(tree);
  if (gold.equals(predicted)) {
    labelsCorrect++;
  }
 else {
    labelsIncorrect++;
  }
  labelConfusion[gold][predicted]++;
}
