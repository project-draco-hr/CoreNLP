{
  this.hour=h;
  this.minute=m;
  this.second=s;
  this.millis=ms;
  this.halfday=halfday;
  millis=Math.max(0,millis);
  second+=millis / 1000;
  millis=millis % 1000;
  second=Math.max(0,second);
  minute+=second / 60;
  second=second % 60;
  minute=Math.max(0,minute);
  hour+=hour / 60;
  minute=minute % 60;
  hour=Math.max(0,hour);
  initBase();
}
