{
  if (token.endsWith("/.")) {
    if (next != null && (next.equals("''/''") || next.endsWith("/)"))) {
      return false;
    }
 else {
      return true;
    }
  }
 else   if (token.equals("''/''") || token.endsWith("/)")) {
    if (prev != null && prev.endsWith("/.") && (next == null || (!Character.isLowerCase(next.charAt(0))))) {
      return true;
    }
 else {
      return false;
    }
  }
 else {
    return false;
  }
}
